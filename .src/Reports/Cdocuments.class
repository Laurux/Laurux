    If Settings["/Soc" & Start.Societe & "/AutoEnt"] Or If Settings["/Soc" & Start.Societe & "/Franchise"] Then
      Me.cell(20, 6, "PU", True, 0, "C", True)
      Me.cell(17, 6, "Qté", True, 0, "C", True)
