' Gambas class file

R As Integer
G As Integer
B As Integer
R2 As Integer
G2 As Integer
B2 As Integer
Coln As Boolean
Inherits PdfWriter

Public Sub _new()

  R = Start.R
  G = Start.G
  B = Start.B
  R2 = Start.R2
  G2 = Start.G2
  B2 = Start.B2
  If R2 = 0 Then
    R2 = R
    G2 = G
    B2 = B
  Endif

End

