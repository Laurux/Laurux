Public Sub Logo3()

  Dim $logo As String

  If Settings["/Soc" & Start.Societe & "/glogo3"] Or If Variables.EnvoiMail = "Mail" Then
    Try $Logo = Settings["/Soc" & Start.Societe & "/Logo2"]
    If Not Error Then
      If Not IsNull($Logo) Then
        Me.Image($Logo, 5, 5, 100, 88)
      Endif
    Endif
  Endif

End

Public Sub Logo2()

  Dim $logo As String

  If Settings["/Soc" & Start.Societe & "/glogo"] Then
    Try $Logo = Settings["/Soc" & Start.Societe & "/Logo"]
    If Not Error Then
      If Not IsNull($Logo) Then
        Me.Image($Logo, 136, 6, 40, 40)
      Endif
    Endif
  Endif

End

Public Sub Logo()

  Dim $logo As String
  Dim hImage As Image
  Dim L As Integer = 80
  Dim h As Integer = 40
  Dim L2 As Integer
  Dim x As Integer = 116
  Dim Y As Integer = 6

  If Settings["/Soc" & Start.Societe & "/glogo"] Or If Variables.EnvoiMail = "Mail" Then
    Try $Logo = Settings["/Soc" & Start.Societe & "/Logo"]
    Try hImage = Image.Load(Settings["/Soc" & Start.Societe & "/Logo"])
    Try L2 = hImage.Width / 10
    If Not Error Then
      If Not IsNull($Logo) Then
        Me.Image($Logo, X + L / L2, Y + h / hImage.Height, hImage.Width / 5, hImage.Height / 5)
      Endif
    Endif
  Endif

End

