                  If RemResult.Available Then
                    If Not IsNull(LigResult!mht) Then
                      BrutA = LigResult!mht
                      TotA = RemResult!mht
                      QteA = LigResult!qte
                    Else
                      BrutA = Bht
                      TotA = Bht
                      Qtea = "1"
                    Endif
