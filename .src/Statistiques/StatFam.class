  rResult = Utils.db.Exec("SELECT * FROM " & Tab3 & " ")
  If rResult.Available Then
    hForm = New VisFam(Me, Tot, Start.Societe, son, dteN0.Text, dteN1.Text)
    hForm.Show()
  Else
    Message.Warning("Aucune donnée pour cette période !", "OK")
  Endif

End
