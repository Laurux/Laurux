Public Sub dte_KeyPress()

  Select Case Last.tag

    Case 1
      If Key.code = Key.enter Or Key.code = Key.Return Or Key.code = Key.Tab Then
        dteN0_LF()
        DteN1.SetFocus
        DteN1.Select
        Stop Event
      Endif

    Case 2
      If Key.code = Key.enter Or Key.code = Key.return Or Key.code = Key.Tab Then
        DteN1_LF()
        dteN0.SetFocus
        dteN0.Select
        Stop Event
      Endif
  End Select

End

