Public Sub SelectionArt()

  Dim sel As String = ""
  Dim MyForm As TriArticles

  Tab = "Fiches_Art"
  sel = ""
  Tri = "art_code"
  MyForm = New TriArticles(True, "", Tri, "Histovart", "", "")
  MyForm.Showmodal()
  If Bsel = True Then
    b = 0
    If Origine = "1" Then
      Art.text = Codearticle
      Art2.SetFocus
      Art2.Select
    Else
