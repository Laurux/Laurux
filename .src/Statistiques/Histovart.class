Public Sub fourman(Cfour As String, Org As String)

  Dim Rfours As Result

  Tab = "Fiches_Four"
  Rfours = Utils.db.Exec("SELECT * FROM " & tab & " WHERE fo_code = &1", Cfour)
  If Not Rfours.Available Then
    If Start.son Then
      Music.Play
    Endif
    Message.Warning("Ce fournisseur n'existe pas !")
    If Org = "Cfour" Then
      Four.Clear
      Four.SetFocus
    Else
      Four2.Clear
      Four2.SetFocus
    Endif
  Endif

End
