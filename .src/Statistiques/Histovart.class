Public Sub Btn_Click()

  Select Case Last.tag

    Case 1
      Button1_Click()

    Case 2
      Report("Button2")

    Case 3
      If Exist(filename) Then Try Kill filename
      Me.Close

    Case 4
      Report("Button4")
  End Select

End

Public Sub Btn_KeyPress()

  Select Case Last.tag
    Case 2
      Report("Button2")
  End Select

End

Public Sub Cmpt_KeyPress()

  If Key.code = Key.enter Or Key.code = Key.return Or Key.code = Key.Tab Then
    Select Case Last.tag
      Case 3
        Art2.SetFocus
        Art2.Select
        Stop Event

      Case 4
        fam.SetFocus
        fam.Select
        Stop Event

      Case 5
        fam2.SetFocus
        fam2.Select
        Stop Event

      Case 6
        four.SetFocus
        four.Select
        Stop Event

      Case 7
        four2.SetFocus
        four2.Select
        Stop Event

      Case 8
        Button2.SetFocus
        Stop Event
    End Select
  Endif

  If key.code = key.F1 Then
    Button1_Click()
    Stop Event
  Endif

  If key.code = key.F2 Then
    Select Case Last.tag
      Case 1
        ToggleButton1_Click()
        Stop Event
      Case 2
        ToggleButton2_Click()
        Stop Event
      Case 3
        ToggleButton3_Click()
        Stop Event
      Case 4
        ToggleButton4_Click()
        Stop Event
      Case 5
        ToggleButton5_Click()
        Stop Event
      Case 6
        ToggleButton6_Click()
        Stop Event
    End Select
  Endif

End

