Public Sub Report(Org As String)

  Dim rResult As Result
  Dim ArtResult As Result
  Dim HResult As Result
  Dim EntkResult As Result
  Dim rcli As Result
  Dim Tab As String
  Dim code As String
  Dim intitule As String
  Dim Fami As String
  Dim four3 As String
  Dim ClientCaisse As String
  Dim Pvht As Float
  Dim dte As LDate
  Dim qte As Float
  Dim Tab2 As String
  Dim Tab3 As String
  Dim Tab4 As String
  Dim Tab5 As String
  Dim Tab6 As String
  Dim Tab7 As String
  Dim Tab8 As String
  Dim Tab9 As String
  Dim $stocke As String
  Dim Nbpage As Integer
  Dim Ligne As Integer = 0
  Dim Bht, codea As String
  Dim Bflag As Boolean = False
  Dim pdf As Chistoriques
  Dim Stocke, Stocka As Boolean = False
  Dim Pb As ProgBar


  Nbpage = 1
  dte = New LDate(Now)
  dte0 = LDate(DteN0.text)
  dte1 = LDate(DteN1.text)
  Tab = "Fiches_Art"
  Tab2 = "Fiches_HistoLigfac"
  Tab3 = "Fiches_HistoLigfacM"
  Tab4 = "Fiches_HistoFac"
  Tab5 = "Fiches_HistoFacM"
  Tab6 = "Fiches_Bl"
  Tab7 = "Fiches_BlM"
  Tab8 = "Fiches_Ligbl"
  Tab9 = "Fiches_LigblM"
  qte = 0
  Pvht = 0
  If RadioButton5.value Then
    Stocke = True
    $stocke = " stockés "
  Else
    Stocke = False
    $Stocke = " non stockés "
  Endif
  Shell "cd " & User.Home & ""
  If Org = "Button2" Then
    Filename = User.Home & "/tmp/HistoArt.pdf"
    pdf = New Chistoriques("Portrait", "mm", "A4")
    pdf.Open()
    pdf.AliasNbPages()
    If Not Devis.value Then
      pdf.Entete("Impression des historiques des ventes de produits" & $stocke & "du " & Dte0.L & " au " & Dte1.L)
    Else
      pdf.Entete("Impression des produits" & $stocke & "présents dans les devis")
    Endif
    pdf.Level1()
  Else
    Filename = User.Home & "/tmp/HistoArt.csv"
    If Exist(Filename) Then Kill Filename
    hFile = Open Filename For Write Create
    If bl.value Then
      Donnees1("Impression des historiques des ventes de produits" & $stocke & " du " & Dte0.L & " au " & Dte1.L)
    Else
      Donnees1("Impression des produits" & $stocke & "présents dans les devis")
    Endif
  Endif
  posx = 2
  posy = 22
  Me.Mouse = Mouse.Wait
  With Utils
    If Not Devis.value Then
      If Radiobutton1.Value Then
        rResult = Utils.db.Exec("SELECT * FROM " & Tab & " WHERE art_code >= &1  and art_code <= &2 and art_fam >= &3 and art_fam <= &4 and art_four >= &5 and art_four <= &6 and art_stocke = &7 order by CAST(art_code AS char)", Art.Text, Art2.Text, Fam.Text, Fam2.Text, four.Text, four2.Text, stocke)
      Endif
      If Radiobutton2.Value Then
        rResult = Utils.db.Exec("SELECT * FROM " & Tab & " WHERE art_code >= &1  and art_code <= &2 and art_fam >= &3 and art_fam <= &4  and art_four >= &5 and art_four <= &6 and art_stocke = &7 order by art_fam, CAST(art_code AS char)", Art.Text, Art2.Text, Fam.Text, Fam2.Text, four.Text, four2.Text, stocke)
      Endif
      If Radiobutton3.Value Then
        rResult = Utils.db.Exec("SELECT * FROM " & Tab & " WHERE art_code >= &1  and art_code <= &2 and art_fam >= &3 and art_fam <= &4  and art_four >= &5 and art_four <= &6 and art_stocke = &7 order by art_four, CAST(art_code AS char)", Art.Text, Art2.Text, Fam.Text, Fam2.Text, four.Text, four2.Text, stocke)
      Endif
      If Radiobutton4.Value Then
        rResult = Utils.db.Exec("SELECT * FROM " & Tab & " WHERE art_code >= &1  and art_code <= &2 and art_fam >= &3 and art_fam <= &4  and art_four >= &5 and art_four <= &6 and art_stocke = &7 order by art_design", Art.Text, Art2.Text, Fam.Text, Fam2.Text, four.Text, four2.Text, stocke)
      Endif
      If rResult.Available Then
        Pb = New ProgBar("Statistiques", rResult.Count, Null, True, "", True)
        PB.Show()
        Repeat
          If Pb.Avancement("Article " & rResult!art_code, 10) = True Then Break
          qte = 0
          Pvht = 0
          code = rResult!art_code
          intitule = rResult!art_design
          fami = rResult!art_fam
          four3 = rResult!art_four
          If Not Devis.value Then
            'On récupère les données archivées.
            If Facn.value = True Then
              HResult = Utils.db.Exec("SELECT * FROM " & Tab2 & " as e inner join " & Tab4 & " as a on a.numfac = e.num_ligfac WHERE code_ligfac = &1 and typel_ligfac = &2 and datefac >= &3 and datefac <= &4", code, "A", Dte0.D, Dte1.D)
              If Hresult.Available Then
                Repeat
                  If Not IsNull(Hresult!qte_ligfac) Then
                    Try qte = qte + Val(.cpoint(Hresult!qte_ligfac))
                    Try pvht = pvht + Val(.cpoint(Hresult!netht_ligfac))
                  Endif
                Until HResult.MoveNext()
              Endif
            Endif
            If Face.value = True Then
              HResult = Utils.db.Exec("SELECT * FROM " & Tab3 & " as e inner join " & Tab5 & " as a on a.numfac = e.num_ligfac WHERE code_ligfac = &1 and typel_ligfac = &2 and datefac >= &3 and datefac <= &4", code, "A", Dte0.D, Dte1.D)
              If Hresult.Available Then
                Repeat
                  If Not IsNull(Hresult!qte_ligfac) Then
                    Try qte = qte + Val(.cpoint(Hresult!qte_ligfac))
                    Try pvht = pvht + Val(.cpoint(Hresult!netht_ligfac))
                  Endif
                Until HResult.MoveNext()
              Endif
            Endif
            If Fact.value = True Then
              HResult = Utils.db.Exec("SELECT * FROM " & Tab2 & " as e inner join " & Tab4 & " as a on a.numfac = e.num_ligfac WHERE code_ligfac = &1 and typel_ligfac <> &2 and datefac >= &3 and datefac <= &4", code, "A", Dte0.D, Dte1.D)
              If Hresult.Available Then
                Repeat
                  If Not IsNull(Hresult!qte_ligfac) Then
                    qte = qte + Val(.cpoint(Hresult!qte_ligfac))
                    Try pvht = pvht + Val(.cpoint(Hresult!netht_ligfac))
                  Endif
                Until HResult.MoveNext()
              Endif
            Endif
            Bflag = False
            'On récupère les tickets Caisse uniquement
            'If HBox3.Visible = True Then
              If Fact.value = True Or If Factk.value = True Or If Fact2.value = True Or If Factk2.value = True Then
                  rcli = Utils.db.Exec("SELECT * FROM " & Cbase.Table("TabCliCaisse") & "")
                  If rcli.Available Then
                    ClientCaisse = rcli!code
                  Else
                    'default client caisse
                    ClientCaisse = "411027"
                  Endif
                HResult = Utils.db.Exec("SELECT * FROM " & Cbase.Table("TabHisLigTck") & " WHERE code = &1 AND type = &2 ", code, "A")
                If HResult.Available Then
                  Repeat
                    EntkResult = Utils.db.Exec("SELECT * FROM " & Cbase.Table("TabHisEntTck") & " WHERE numero = &1 and suppr = &2 and client = &3 and date >= &4 and date <= &5", HResult!numero, 0, ClientCaisse, Dte0.D, Dte1.D)
                    If EntkResult.Available Then
                      qte = qte + Val(.cpoint(Hresult!qte))
                      If IsNull(Hresult!mht) Then
                        ArtResult = Utils.db.Exec("SELECT * FROM " & Cbase.Table("TabArt") & " Where art_code = &1", code)
                        If ArtResult.Available Then
                          Bht = ArtResult!art_pvht
                        Else
                          Bht = "0"
                        Endif
                        pvht = pvht + Val(.cpoint(Bht))
                      Else
                        pvht = pvht + Val(.cpoint(Hresult!mht))
                      Endif
                    Endif
                  Until HResult.MoveNext()
                Endif
              Endif
            'Endif
            Bflag = False
            'On récupère les données en cours.

            If (Facn.value = True Or Factk.value = True) And bl.value Then
              HResult = Utils.db.Exec("SELECT * FROM " & Tab8 & " as e inner join " & Tab6 & " as a on a.numbl = e.num_ligbl WHERE code_ligbl = &1 and typel_ligbl = &2 and datebl >= &3 and datebl <= &4", code, "A", Dte0.D, Dte1.D)
              If HResult.Available Then
                Repeat
                  'On travaille avec les BL
                  If Hresult!type = "B" Or If Hresult!type = "F" Then
                    If Not IsNull(Hresult!qte_ligbl) Then
                      qte = qte + Val(.cpoint(Hresult!qte_ligbl))
                      pvht = pvht + Val(.cpoint(Hresult!netht_ligbl))
                    Endif
                  Endif
                Until HResult.MoveNext()
              Endif
            Endif
            If Face.value = True And bl.value Then
              HResult = Utils.db.Exec("SELECT * FROM " & Tab9 & " as e inner join " & Tab7 & " as a on a.numbl = e.num_ligbl WHERE code_ligbl = &1 and typel_ligbl = &2 and datebl >= &3 and datebl <= &4", code, "A", Dte0.D, Dte1.D)
              If HResult.Available Then
                Repeat
                  'On travaille avec les BL
                  If Hresult!type = "B" Or If Hresult!type = "F" Then
                    If Not IsNull(Hresult!qte_ligbl) Then
                      qte = qte + Val(.cpoint(Hresult!qte_ligbl))
                      pvht = pvht + Val(.cpoint(Hresult!netht_ligbl))
                    Endif
                  Endif
                Until HResult.MoveNext()
              Endif
            Endif
            If Fact.value = True And bl.value Then
              HResult = Utils.db.Exec("SELECT * FROM " & Tab8 & " as e inner join " & Tab6 & " as a on a.numbl = e.num_ligbl WHERE code_ligbl = &1 and typel_ligbl = &2 and datebl >= &3 and datebl <= &4 union SELECT * FROM " & Tab9 & " as e inner join " & Tab7 & " as a on a.numfac = e.num_ligbl  WHERE code_ligbl = &1 and typel_ligbl = &2 and datebl >= &3 and datebl <= &4", code, "A", Dte0.D, Dte1.D)
              If HResult.Available Then
                Repeat
                  'On travaille avec les BL
                  If Hresult!type = "B" Or If Hresult!type = "F" Then
                    If Not IsNull(Hresult!qte_ligbl) Then
                      qte = qte + Val(.cpoint(Hresult!qte_ligbl))
                      Try pvht = pvht + Val(.cpoint(Hresult!netht_ligbl))
                    Endif
                  Endif
                Until HResult.MoveNext()
              Endif
            Endif
          Else
            HResult = Utils.db.Exec("SELECT * FROM " & Tab8 & " as e inner join " & Tab6 & " as a on a.numbl = e.num_ligbl WHERE code_ligbl = &1 and typel_ligbl = &2 and type = &3 and datebl >= &3 and datebl <= &4", code, "A", "D", Dte0.D, Dte1.D)
            If Hresult.Available Then
              Bflag = True
            Else
              Bflag = False
            Endif
          Endif
          If Org = "Button2" Then
            If Val(.cpoint(qte)) <> 0 Then
              pdf.level2(code, intitule, fami, four3, qte, Format$(pvht, "0.00"), PosY)
              PosY += 5
              ligne = 1
            Endif
            If PosY >= 283 Then
              pdf.Footer()
              PosY = 22
              pdf.Entete("Impression des historiques des ventes de produits" & $stocke & " du " & Dte0.L & " au " & Dte1.L)
              pdf.Level1()
            Endif
          Else
            If Val(.cpoint(qte)) <> 0 Then
              Donnees2(code, intitule, fami, four3, qte, Format$(pvht, "0.00"))
              ligne = 1
            Endif
          Endif
        Until rResult.MoveNext()
        PB.Close()
      Endif
    Else
      HResult = Utils.db.Exec("SELECT * FROM " & Tab6 & " as e inner join " & Tab8 & " as a on a.num_ligbl = e.numbl WHERE type = &1 and typel_ligbl = &2 order by CAST(code_ligbl AS char)", "D", "A")
      If HResult.Available Then
        Pb = New ProgBar("Statistiques Devis", HResult.Count, Null, True, "", True)
        PB.Show()
        code = Hresult!code_ligbl
        Repeat
          If Pb.Avancement("Article " & HResult!code_ligbl, 1) = True Then Break
          If Hresult!code_ligbl <> code Then
            If stocka = stocke And four3 >= four.Text And four3 <= four2.Text Then
              If Org = "Button2" Then
                If Val(.cpoint(qte)) <> 0 Then
                  pdf.level2(code, intitule, fami, four3, qte, Format$(pvht, "0.00"), PosY)
                  PosY += 5
                  ligne = 1
                Endif
                If PosY >= 283 Then
                  pdf.Footer()
                  PosY = 22
                  pdf.Entete("Impression des produits" & $stocke & "présents dans les devis")
                  pdf.Level1()
                Endif
              Else
                If Val(.cpoint(qte)) <> 0 Then
                  Donnees2(code, intitule, fami, four3, qte, Format$(pvht, "0.00"))
                  ligne = 1
                Endif
              Endif
            Endif
            rResult = Utils.db.Exec("SELECT * FROM " & Tab & " WHERE art_code = &1", Hresult!code_ligbl)
            If rResult.Available Then
              Qte = 0
              Pvht = 0
              code = rResult!art_code
              intitule = rResult!art_design
              fami = rResult!art_fam
              four3 = rResult!art_four
              qte = qte + Val(.cpoint(Hresult!qte_ligbl))
              Stocka = rResult!art_stocke
            Endif
          Else
            codea = code
            If Not IsNull(Hresult!qte_ligbl) Then
              qte = qte + Val(.cpoint(Hresult!qte_ligbl))
              pvht = pvht + Val(.cpoint(Hresult!netht_ligbl))
            Endif
          Endif
        Until HResult.MoveNext()
        PB.Close()
      Endif
      If Org = "Button2" Then
        If Val(.cpoint(qte)) <> 0 Then
          pdf.level2(code, intitule, fami, four3, qte, Format$(pvht, "0.00"), PosY)
          PosY += 5
          ligne = 1
        Endif
        If PosY >= 283 Then
          pdf.Footer()
          PosY = 22
          pdf.Entete("Impression des produits & $stocke &  présents dans les devis")
          pdf.Level1()
        Endif
      Else
        If Val(.cpoint(qte)) <> 0 Then
          Donnees2(code, intitule, fami, four3, qte, Format$(pvht, "0.00"))
          ligne = 1
        Endif
      Endif
    Endif
    If ligne = 1 Then
      If Org = "Button2" Then
        pdf.Footer()
        pdf.Output(Filename, False)
        Visualiseur.Prog_Editeur(Filename)
      Else
        Close #hFile
        If Message.Question("Le fichier csv a été généré. Voulez-vous l'ouvrir ?", "Oui", "Non") = 1 Then
          Editeur.Prog_Editeur(Filename)
        Endif
      Endif
    Else
      If son Then
        Music.Play
      Endif
      message(" Aucun article pour cette demande", "OK")
    Endif
    Me.Mouse = Mouse.Pointing
  End With

End

Public Sub donnees1(p1 As String)

  Print #hFile, p1

End

Public Sub donnees2(p1 As String, p2 As String, p3 As String, p4 As String, p5 As String, p6 As String)

  Print #hFile, p1 & ";" & p2 & ";" & p3 & ";" & p4 & ";" & p5 & ";" & p6

End


