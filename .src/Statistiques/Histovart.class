Public Sub Dte_Check()

  If Not dte0.Avant(dte1) Then
    If son Then
      Music.Play
    Endif
    If Message.Warning("Attention ! Votre selection n'est pas possible.", "Ok") = 1 Then
      dte1 = dte0
      DteN0.Text = dte0.L
      DteN1.Text = dte1.L
    Endif
  Endif

End

