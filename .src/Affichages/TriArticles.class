Public Sub _new(Barticle As Boolean, B2article As Boolean, TriArticle As String, sOrg As String, sFour As String, Centrale As String, Optional Dispo As Boolean = False, Optional Mat As Boolean = False)

  Utils.Observers(Me)
  lg = Int(Colart.height / colart.Rows.Height) - 1
  Me.Center
  Origine = sOrg
  If IsNull(sFour) Then CB2.Text = ""
  If Origine = "Commande" And If Not IsNull(sFour) Then
    CB2.Text = sFour
  Else
    If Origine = "ModifCom" And If Not IsNull(sFour) Then
      CB2.Text = sFour
    Else
      If Origine = "Receipt" And If Not IsNull(sFour) Then
        CB2.Text = sFour
      Else
        If Origine = "Receipta" And If Not IsNull(sFour) Then
          CB2.Text = sFour
        Else
          $Org = "C"
          Barticle = True
        Endif
      Endif
    Endif
  Endif
  $Centrale = Centrale
  Bart = Barticle
  B2art = B2article
  Tri2Art = TriArticle
  If Start.LocalSettings["/Soc" & Start.Societe & "/Materiel"] = True Then
    Materiel.Value = Mat
    Materiel.Visible = True
  Else
    Materiel.Value = False
    Materiel.Visible = False
  Endif
  Disponible.Value = Dispo
  Filter_Calc()
  Triart()
  If $Org = "C" And If Not IsNull(CB2.Text) Then
    FourMan()
    org = "Fournisseur"
    Deb7()
  Else
    If $Org = "C" And If IsNull(CB2.Text) Then
      Deb7()
    Else
      Deb5()
      CB2.Text = ""
    Endif
  Endif
  Cb1.Text = "Equivalent"
  CO.Text = "Code"
  It.Text = "Intitulé"
  Colart.MoveTo(0, 0)
  'Try colart.Current.EnsureVisible
  Colart.SetFocus

End

