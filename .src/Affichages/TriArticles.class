Public Sub CO_GotFocus()

  Try Utils.ObsGotf(Co)
  CO.Select
  CO.SetFocus
  chkfocus = False
  sel = ""
  Cb1.Text = "Equivalent"
  CO.Text = ""
  It.Text = "Intitulé"
  Tri2Art = "art_code"
