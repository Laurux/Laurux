Public Sub CO_KeyPress()

  Chkinput()
  If Key.code = Key.Escape Or Key.code = Key.F2 Then Me.close()
  If key.code = key.Down Then
    Colart.SetFocus
    Colart.MoveTo(0, 0)
  Endif
  If key.code = key.Right Then IT.SetFocus

End

