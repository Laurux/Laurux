Public Sub Cb1_GotFocus()

  Try Utils.ObsGotf(Cb1)
  If chkfocus = True Then
    sel = ""
    Select Case Cb1.Text
      Case "Equivalent"
        Tri2Art = "art_cequ"
        'Case "Fournisseur"
        'Tri2Art = "art_cfour"
      Case "Barre"
        Tri2Art = "art_cbarre"
        'Case "Centrale"
        'Tri2Art = "art_refcentrale"
      Case "CN8"
        Tri2Art = "art_cn8"
      Case "Casier"
        Tri2Art = "art_casier"
      Case "Caracteristique"
        Tri2Art = "art_crst"
      Case Else
        Tri2Art = "art_cequ"
    End Select
    It.Text = "Intitulé"
    CO.Text = "Code"
    NbEnreg = 0
    Dbl = ""
    If $Org = "C" Then
      Deb5()
    Else
      If Not IsNull(CB2.text) Then
        Deb1()
      Else
        Deb5()
      Endif
    Endif
    Cb1.Text = ""
    Cb1.Select
  Endif

End

