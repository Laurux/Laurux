Public Sub Cmpt_LostFocus()

  Try hder = Last
  Select Case Last.tag

    Case 2
      If LF = 0 Then
        Datej_LostFocus()
        If B = 1 Then
          Datej_GotFocus()
          Datej.SetFocus
          datej.Select
          Stop Event
        Else
          b = 0
          numcompte_GotFocus()
          numcompte.SetFocus
          numcompte.Select
        Endif
      Endif
      Stop Event

    Case 3
      If soldeecrit.text <> "0,00" And Not IsNull(numcompte.text) Or If soldeecrit.text = "0,00" And Not IsNull(numcompte.text) And Ecrit1.count = 0 Then
        If LF = 0 Then
          If Not IsNull(numcompte.text) Then
            Type_compte()
            If Bclient.Value Or If Bfournisseur.Value Then
              compteman()
            Else
              compteGman()
            Endif
            If B = 1 Then
              numcompte.SetFocus
              numcompte_GotFocus()
              numcompte.Select
              B = 0
            Else
              If b = 0 Then
                Ncpt = Numcompte.text
                Libelecrit_GotFocus()
                Libelecrit.SetFocus
                Libelecrit.Select
              Else
                If b = 2 Then
                  Journal.SetFocus
                Else
                  If b = 3 Then
                    Datej.SetFocus
                  Endif
                Endif
              Endif
            Endif
          Endif
          Stop Event
        Endif
      Else
        If soldeecrit.text = "0,00" And Not IsNull(numcompte.text) And Ecrit1.count > 0 Then
          Message.Warning("Cette écriture est soldée ! Saisie impossible.")
          Numcompte.SetFocus
          Numcompte.Select
        Endif
      Endif
    Case 4
      $libelle = Libelecrit.Text
      Stop Event
  End Select

End

Public Sub Cmpt_KeyPress()

  Dim hForm2 As Extrait
