    .Columns[5].Alignment = 2
    .Columns[5].Text = "Crédit"
    .Columns[6].Alignment = 2
    .Columns[6].Text = "Solde"
    .Columns[7].Alignment = 2
    .Columns[7].Text = "Numéro"
    .Columns[8].Alignment = 3
    .Columns[8].Text = "Journal"
