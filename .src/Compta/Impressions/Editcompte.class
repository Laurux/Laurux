Public Sub _New()

  Utils.Observers(Me)

  filetxt = User.Home & "/tmp/Glivre.txt"
  Music.Load(Start.Musique)
  Me.Center
  E0_click()
  cpt.Text = "41100001"
  cpt2.Text = "411999999"
  type = "C"
  jour = Format$(Now, "dd.mm.yyyy")
  Exo()
  If Right$(jour, 4) & Mid$(jour, 4, 2) & Left$(jour, 2) > Right$(dteN1.Text, 4) & Mid$(dteN1.Text, 4, 2) & Left$(dteN1.Text, 2) Then
    Ex1()
    dteN1.Text = jour
  Else
    exo()
  Endif
  E0.Value = True
  dteN0.SetFocus

End

'*****************************************
'* On travaille avec l'exercice en cours *
'*****************************************
Public Sub Ex1()

  Dim Tab As String

  Tab = "Fiches_Parametres"
  With utils
    rResult = Utils.db.Exec("SELECT * FROM " & Tab & "")
    dte = rResult!dteclec
    dteN0.Text = .Calc_mois(dte)
    jour = Format$(Now, "dd.mm.yyyy")
    If Right$(jour, 4) & Mid$(jour, 4, 2) & Left$(jour, 2) < Right$(dteN0.Text, 4) & Mid$(dteN0.Text, 4, 2) & Left$(dteN0.Text, 2) Then
      Exo()
    Else
      dteN1.Text = jour
      dte1 = dteN0.Text
      dte2 = dteN1.Text
    Endif
  End With

End

