Public Sub Deb2()

  If T.Value = True Then Utils.Base(ColLcr, "select * from Fiches_Lcr where banque = " & "0" & "  and bordereau <> " & "1" & " order by " & Tri & "")
  If A.Value = True Then Utils.Base(ColLcr, "select * from Fiches_Lcr where acceptee = " & 1 & " and banque = " & "0" & "  and bordereau = " & 0 & " order by " & Tri & "")
  If E.Value = True Then Utils.Base(ColLcr, "select * from Fiches_Lcr where ecartee = " & 1 & "  and banque = " & "0" & "  and bordereau = " & 0 & " order by " & Tri & "")

End

Public Sub Deb3()

  If T.Value = True Then Utils.Base(ColLcr, "select * from Fiches_Lcr order by " & Tri & " Desc")
  If A.Value = True Then Utils.Base(ColLcr, "select * from Fiches_Lcr where " & Tri & " like  \"" & sel & "%\" and acceptee = " & 1 & "  and bordereau = " & 0 & " order by " & Tri & " Desc")
