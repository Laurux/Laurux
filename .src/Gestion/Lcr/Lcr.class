Public Sub CleanChamps()

  Code.clear
  Nom.Clear
  dte.Clear
  Datee.Clear
  Numfac.Clear
  Montant.Clear
  cAcc.Value = False
  cRej.Value = False

End

Public Sub Maj_Zones()

  Dim rLcr As Result

  With Utils
    code.Text = ColLcr[ColLcr.row, 0].Text
    rLcr = .db.Exec("select * from Fiches_Lcr where lind = &1", ColLcr[ColLcr.row, 8].Text)
    If rLcr.Available Then
      Nom.Text = rLcr!nom
      dte.dte.G = rLcr!date
      Datee.dte.G = rLcr!datech
      Numfac.Text = rLcr!numfac
      Montant.text = rLcr!montant
      Try cAcc.Value = rLcr!acceptee
      Try cRej.Value = rLcr!ecartee
    Endif
  End With
Catch
  Music.Play
  message.Error(Error.Text & " " & Error.where)

End

