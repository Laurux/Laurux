Public Sub DAE_KeyPress()

  If key.code = key.backspace Then
    sel = ""
    DAE.clear
    Deb2()
  Else
    sel = sel & key.Text
    If Len(sel) = 4 Then
      sel = "%-" & Right$(sel, 2) & "-" & Left$(sel, 2)
      Deb4()
    Else
      If Len(sel) = 8 Then
        sel = Right$(sel, 2) & "-" & Mid$(sel, 3, 2) & "-" & Left$(sel, 2)
        Deb4()
      Endif
    Endif
  Endif

End

