    Utils.base(ColLcr, "SELECT * FROM Fiches_Lcr  where banque = " & Left$(BQ.Text, 1) & " and bordereau = " & 0 & " and datech >= " & echeance1 & " and datech <= " & echeance2 & "  AND codeentree = " & Left$(Entree3.text, 1) & "")
