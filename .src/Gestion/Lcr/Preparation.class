Public Sub Deb()

  Enrg = 0
  If IsNull(BQ.Text) Then
    Utils.base(ColLcr, "SELECT * FROM Fiches_Lcr  where bordereau = " & 0 & "")
    Der = Utils.db.Exec("SELECT Count(*) as nb  FROM Fiches_Lcr  where bordereau = " & 0 & "")
    Enrg = der!nb
  Else
    If IsNull(Entree.Text) Then
      Utils.base(ColLcr, "SELECT * FROM Fiches_Lcr  where banque = " & Left$(BQ.Text) & " and codeentree = " & 0 & " and bordereau = " & 0 & "")
      Der = Utils.db.Exec("SELECT Count(*) as nb  FROM Fiches_Lcr  where banque = " & Left$(BQ.Text) & " and codeentree = " & 0 & " and bordereau = " & 0 & "")
      Enrg = der!nb
    Else
      Utils.base(ColLcr, "SELECT * FROM Fiches_Lcr  where banque = " & Left$(BQ.Text) & " AND codeentree = " & Left$(Entree.text, 1) & " and bordereau = " & 0 & "")
      Der = Utils.db.Exec("SELECT Count(*) as nb  FROM Fiches_Lcr  where banque = " & Left$(BQ.Text) & " AND codeentree = " & Left$(Entree.text, 1) & " and bordereau = " & 0 & "")
      Enrg = der!nb
    Endif
  Endif
  Total()

End

