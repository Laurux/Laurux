Public Sub Form_Open()

  Tri = "date"
  Tris()
  Deb2()
  Total()
  bq.Clear
  BQ2.Clear
  BQ.Add("")
  BQ2.Add("")
  Try rbq = Utils.db.Exec("SELECT * FROM Fiches_Banques ")
  If Not Error Then
    If rBq.Available Then
      Do
        Try BQ.Add(rBq!banques_code & " - " & rBq!banques_nom)
        Try BQ2.Add(rBq!banques_code & " - " & rBq!banques_nom)
      Loop Until rBq.MoveNext()
    Endif
  Endif
  code.SetFocus
  code.Select

End

