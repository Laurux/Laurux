Public Sub Deb4()

  Enrg = 0
  Utils.Base(ColLcr, "select * from Fiches_Lcr  where banque is not null and " & Tri & " like  \"%" & sel & "%\" and bordereau = " & 0 & " or banque = " & "0" & " and ecartee <> " & 1 & " and " & Tri & " like  \"%" & sel & "%\" and bordereau = " & 0 & " order by " & Tri & ", 1")
  Der = Utils.db.Exec("SELECT Count(*) as nb  FROM Fiches_Lcr  where banque is not null and " & Tri & " like  \"%" & sel & "%\" and bordereau = " & 0 & " or banque = " & "0" & " and ecartee <> " & 1 & " and " & Tri & " like  \"%" & sel & "%\" and bordereau = " & 0 & " order by " & Tri & ", 1")
  Enrg = der!nb
  Total()

End

Public Sub Total()

  Dim rLcr As Result
  Dim ikey As Integer = 0
  Dim b As Boolean = False
  Dim Nb As Integer = 0
  Dim Tot As Float = 0

  With Utils
    Repeat
      Try ColLcr.MoveTo(ikey, 0)
      If Not Error Then
        If ikey = Enrg Then
          Break
          Enrg = 0
        Endif
        Try Tot = Tot + Val(Utils.cpoint(ColLcr[ColLcr.row, 5].Text))
        If Error Then
          B = True
        Else
          Inc nb
        Endif
      Else
        B = True
      Endif
      Inc ikey
    Until b <> False
    Nb2.Text = nb
    Mt2.Text = Format$(Tot, "0.00")
  End With
Catch
  Music.Play
  Try message.Error(Error.Text & " " & Error.where)

End

