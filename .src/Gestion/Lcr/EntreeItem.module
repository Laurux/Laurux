' Gambas module file

Public Sub item(sentree As String) As String

  Select Case sentree

    Case 1
      Return "1- Escompte"

    Case 2
      Return "2- Escompte en valeur"

    Case 3
      Return "3- Encaissement crédit forfaitaire"

    Case 4
      Return "4- Encaissement crédit après échéance"

    Case 5
      Return "5- Cession escompte (Loi Dailly)"

  End Select

End
