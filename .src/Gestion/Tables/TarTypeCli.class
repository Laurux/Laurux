Public Sub Button1_Click()

  Doc.Open("TarifClient")

End

Public Sub CO_MouseDown()

  If Not Dbl Then
    sens = " asc"
    Dbl = True
  Else
    sens = " desc"
    Dbl = False
  Endif

  sel = ""
  CO.Clear
  DSG.Text = "Désignation"
  Tri = "cart"
  Aff_Tarart()

End

Public Sub CO_KeyPress()

  If Key.code = Key.Return Or Key.code = Key.Enter Then
  Else
    If key.code = key.backspace Then
      sel = Left$(sel, (Len(sel) - 1))
    Else
      sel = sel & key.Text
    Endif
    Aff_Tarart()
  Endif

End

Public Sub CO_LostFocus()

  CO.Text = "Code"

End

Public Sub DSG_MouseDown()

  If Not Dbl Then
    sens = " asc"
    Dbl = True
  Else
    sens = " desc"
    Dbl = False
  Endif

  sel = ""
  CO.Text = "Code"
  DSG.clear
  Tri = "art_design"
  Aff_Tarart()

End

Public Sub DSG_KeyPress()

  If Key.code = Key.Return Or Key.code = Key.Enter Then
  Else
    If key.code = key.backspace Then
      sel = Left$(sel, (Len(sel) - 1))
    Else
      sel = sel & key.Text
    Endif
