Public Sub Calc_Remise()

  Dim rrr As Float
  'La remise est calculée a partir du pv ht public
  rrr = Val(Ppublic) - Val(Tpvht.text)
  Remise.text = Format$((rrr * 100) / Val(Ppublic), "0.00")

End

