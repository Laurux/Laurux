Public Sub Decimal(Stk As String) As String

  With Utils
    Stk = .CPoint(Stk)
    If DEC.text = "0" Then
      Stk = Format$(Val(Stk), "0")
    Else
      If DEC.text = "2" Then
        Stk = Format$(Val(Stk), "0.00")
      Else
        If dec.Text = "3" Then
          Stk = Format$(Val(Stk), "0.000")
        Endif
      Endif
    Endif
  End With
  Return Stk

End

