    Else
      Colcom.visible = True
      Colcom.Columns.count = 5
      Colcom.Columns[0].Width = 74
      Colcom.Columns[1].Width = 100
      Colcom.Columns[2].Width = 100
      Colcom.Columns[3].Width = 100
      Colcom.Columns[4].Width = 100
