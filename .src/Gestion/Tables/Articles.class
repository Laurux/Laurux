  Dssr.Rows.Count = 0
  Dssr.Columns.Count = 4
  Dssr.Columns[0].Width = 100
  Dssr.Columns[1].Width = 0
  Dssr.Columns[2].Width = 330
  Dssr.Columns[3].Width = 330
