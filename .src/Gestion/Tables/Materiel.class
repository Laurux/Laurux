Public Sub ToggleButton2_Click()
  
  Dim rResult As Result
  
  Tab = "Fiches_Art"
  ColArt.Clear
  If ColArt.Visible Then
    ColArt.Clear
    ColArt.Visible = False
  Else
    ColArt.Visible = True
    With ColArt
      .Columns.count = 3
      .Columns[0].Width = 120
      .Columns[1].Width = 420
      .Columns[2].Width = 160
      .Columns[2].Alignment = 4
      .Columns[0].Text = "Code"
      .Columns[1].Text = "Intitulé"
      .Columns[2].Text = "Ref fournisseur"
    End With
    rResult = Utils.db.Exec("SELECT * FROM " & Tab & " where art_mat = &1 order by CAST(art_code AS char)", 1)
    If rResult.Available Then
      Do
        With utils
          ColArt.Add(rResult!art_code, rResult!art_code)
          ColArt.Item[0] = rResult!art_code
          ColArt.Item[1] = rResult!art_design
          ColArt.Item[2] = rResult!art_cfour
          nbd.Text = rResult!art_nbd
          nbdec = Utils.Find_nbdec(nbd.text)
        End With
      Loop Until rResult.MoveNext()
    Else
      If son Then
        Music.Play
      Endif
      Message.Warning("Il n'existe aucun article stocké \n veuillez d'abord stocker vos références SVP ! ")
      ColArt.Clear
      ColArt.visible = False
    Endif
    If ColArt.Count Then
      ColArt.MoveFirst
      ColArt.SetFocus
      ColArt.Item.Selected = True
    Endif
  Endif
  
End

'*******************************************
'*  Saisie d'un article a la souris        *
'*******************************************
Public Sub Colart_Click()
  
  Codep.Text = ColArt.Current[0]
  ColArt.Clear
  ColArt.visible = False
  art_Man()
  If b = 1 Then
    Codep.SetFocus
    codep.Select
    b = 0
  Else
    Code.SetFocus
    Code.Select
  Endif
  
End

'*******************************************
'* Saisie d'un article manuellement        *
'*******************************************
Public Sub Colart_KeyPress()
  
  If Key.code = Key.Enter Or Key.code = Key.Return Then
    Codep.Text = ColArt.Current[0]
    ColArt.Clear
    ColArt.visible = False
    art_Man()
    If b = 1 Then
      Codep.SetFocus
      codep.Select
      b = 0
    Else
      Code.SetFocus
      Code.Select
    Endif
  Endif
  
  If key.code = key.F1 Then
    Button7_Click()
    Stop Event
  Endif
  
  If Key.code = Key.Escape Or Key.code = Key.F2 Or Key.Code = Key.F3 Then
    ColArt.visible = False
    ColArt.clear
  Endif
  
End

'*************************************************
'* Saisie d'un code article manuellement         *
'*************************************************
Public Sub art_man()
  
  Dim rarts As Result
  Dim Rtvaav As Result
  Dim Rfours As Result
  Dim qtec As Float
  Dim Tab3 As String
  Dim Tab4 As String
  Dim stocke As String
  Dim Cdp As String
  
  b = 0
  Cdp = Codep.text
  Qtec = "0"
  Tab = "Fiches_Art"
  Tab2 = "Fiches_Tvaav"
  Tab3 = "Fiches_Four"
  Tab4 = "Fiches_Four"
  CleanChamps()
  With utils
    rarts = Utils.db.Exec("SELECT * FROM " & Tab & " where art_code = &1", Cdp)
    If rarts.Available Then
      Stocke = rarts!art_stocke
      If Not Stocke Then
        If son Then
          Music.Play
        Endif
        message.Warning(" Ce produit n'est pas stocké ! ")
      Else
        If Not rarts!art_mat Then
          message.Warning(" Ce produit n'est pas un matériel ! ")
        Else
          If rarts!art_suspendu = False Or If IsNull(rarts!art_suspendu) Then
            Codep.Text = rarts!art_code
            Desg.text = rarts!art_design
            Desg2.Text = rarts!art_design2
            Cfour.Text = rarts!art_cfour
            Four.text = Rarts!art_four
            Fam.text = Rarts!art_fam
            Cbarre.text = rarts!art_cbarre
            Cequ.Text = rarts!art_cequ
            nbd.Text = rarts!art_nbd
            nbdec = Utils.Find_nbdec(nbd.text)
            Paht.Text = Format$(rarts!art_paht, nbdec)
            Frais.Text = Format$(rarts!art_frais, nbdec)
            Prvt.Text = Format$(rarts!art_prvt, nbdec)
            Coef.Text = Format$(rarts!art_coef, "0.0000")
            Pvht.Text = Format$(rarts!art_pvht, nbdec)
            Tva.Text = rarts!art_tva
            PvTTc.Text = Format$(rarts!art_pvttc, nbdec)
            Marque.Text = rarts!art_marque
            Crst.text = rarts!art_crst
            Try Eco.Text = Format$(rarts!art_eco, nbdec)
            If Error Then Eco.Text = "0,00"
            Calc_Percent()
            Calc_coeff()
            Arrondi()
            Fam2()
            Arr.Text = rarts!art_cdarr
            Rtvaav = Utils.db.Exec("SELECT * FROM " & Tab2 & " where code_tva = &1", Tva.Text)
            If Rtvaav.Available Then
              Txtva.text = Format$(Rtvaav!taux_tva, "0.00")
            Endif
            Rfours = Utils.db.Exec("SELECT * FROM " & tab3 & " WHERE fo_code = &1", four.text)
            Fournom.Text = Rfours!fo_nom
          Else
            Message.Warning(" Saisie impossible ! Cet article est suspendu. ")
            b = 1
          Endif
        Endif
      Endif
    Endif
  End With
  
End

