Public Sub colinit(Col As GridView, Coln As String)
  
  With Utils
    If sens = "Asc" Then
      sens = "Desc"
    Else
      sens = "Asc"
    Endif
    If coln = 0 Then
      tricli = "cli_code"
      AffClients(tricli, sens, Col)
    Endif
    If coln = 1 Then
      tricli = "cli_nom"
      AffClients(tricli, sens, Col)
    Endif
  End With
  
End

