Public Sub Nom_Photo()
  
  Dim iPos As Integer
  Dim x As Integer
  Dim Pht As String
  
  For x = 1 To Len(Photo.Text)
    iPos = InStr(Photo.Text, "/")
    Pht = Left$(Photo.Text, iPos - 1)
    Photo.Text = Mid$(Photo.Text, iPos + 1)
  Next
  
End

