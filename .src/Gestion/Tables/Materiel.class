Public Sub Tva_Calcul()
  'DIM Mtva AS Float
  
  With Utils
    If PvTTc.Text <> "" Or Pvht.Text <> "" Then
      'TRY Mtva = Val(Txtva.Text)
      'Txtva.Text = Format$(Val(.cpoint(Mtva)), "00.00")
      PvTTc.Text = Format$(Val(Pvht.Text) + (Val(Pvht.Text) * Val(Txtva.Text) / 100), nbdec)
    End If
  End With
  
End

