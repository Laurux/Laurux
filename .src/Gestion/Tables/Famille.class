Public Sub Coef_LostFocus()

  Dim Tx As Float

  With Utils
    Coef.Text = .cpoint(Coef.Text)
    If Val(Coef.text) = Null Then
      If son Then
        Music.Play
      Endif
      message.Warning("Veuillez verifier la saisie de votre coefficient SVP !")
      b = 1
    Else
      Tx = Val(.cpoint(Coef.Text))
      Coef.Text = Format$(Tx, "0.000")
      b = 0
    Endif
  End With

End

