Public Sub Colcompt_KeyPress()

  If Key.code = Key.Enter Or Key.code = Key.Return Then
    Colcompt.MoveCurrent
    Colcompt_Click()
    Stop Event
  Endif
  If Key.code = Key.Escape Or Key.code = Key.F2 Then
    Colcompt.visible = False
    Colcompt.clear
    CmptRem.SetFocus
    CmptRem.Select
    Stop Event
  Endif

