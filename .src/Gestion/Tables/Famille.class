Public Sub Test_Tva()

  Dim rTva As Result

  If AncT <> Cdtva.Text Then
    rTva = Utils.db.Exec("SELECT * FROM " & Cbase.Table("TabArt") & " WHERE art_tva = &1 and art_fam = &2", Anct, cdf.Text)
    If rTva.Available Then
      Try Message.Warning("Il existe des produits dans cette famille. La modification du code Tva n'est pas permise !")
      Cdtva.Text = AncT
      Cdtva.SetFocus
      Cdtva.Select
      b = 1
    Endif
  Endif

End

Public Sub fam_KeyPress()

  Dim rResult As Result

  If Key.code = Key.enter Or Key.code = Key.Return Or Key.code = Key.Tab Then
    Select Case Last.tag

      Case 1
        LibelFam.SetFocus
        LibelFam.Select
        Stop Event

      Case 2
        Libelfam_LostFocus()
        Coef.SetFocus
        Coef.Select
        Stop Event

      Case 3
        Coef_LostFocus()
        If b = 1 Then
          Coef.SetFocus
          Coef.Select
          b = 0
        Else
          Remise.SetFocus
          Remise.Select
        Endif
        Stop Event

      Case 4
        Remise_LostFocus()
        If b = 1 Then
          Remise.SetFocus
          Remise.Select
          b = 0
        Else
          Cdtva.SetFocus
          Cdtva.Select
        Endif
        Stop Event

      Case 5
        Test_Tva()
        If b <> 1 Then
          rResult = Utils.db.Exec("SELECT * FROM " & Cbase.Table("TabTvav") & " where code_tva = &1", Cdtva.Text)
          If rResult.Available Then
            Txtva.text = Format$(rResult!taux_tva, "0.00")
            codeCompt.SetFocus
            codeCompt.Select
          Else
            Message.Warning("Ce Taux n'existe pas")
            Cdtva.SetFocus
            Cdtva.Select
          Endif
        Endif
        Stop Event

      Case 6
        codeCompt_LostFocus()
        If b = 1 Then
          codeCompt.SetFocus
          codeCompt.Select
        Else
          codeCompt2.SetFocus
          codeCompt2.Select
        Endif
        Stop Event

      Case 7
        codeCompt2_LostFocus()
        If b = 1 Then
          codeCompt2.SetFocus
          codeCompt2.Select
        Else
          codeCompt4.SetFocus
          codeCompt4.Select
        Endif
        Stop Event

      Case 8
        codeCompt4_LostFocus()
        If b = 1 Then
          codeCompt4.SetFocus
          codeCompt4.Select
        Else
          Cmptrem.SetFocus
          Cmptrem.Select
        Endif
        Stop Event

      Case 9
        If Left$(Cmptrem.text, 3) = "709" Then
          Cmptrem_LostFocus()
          If b = 1 Then
            CmptRem.SetFocus
            CmptRem.Select
            b = 0
          Else
            codeCompt3.SetFocus
            codeCompt3.Select
          Endif
        Else
          Message.Warning("Un compte de remise doit être un compte 709xxx")
          CmptRem.SetFocus
          CmptRem.Select
        Endif
        Stop Event

      Case 10
        codeCompt3_LostFocus()
        If b = 1 Then
          codeCompt3.SetFocus
          codeCompt3.Select
        Else
          Ecotx.SetFocus
          Ecotx.Select
        Endif
        Stop Event

      Case 11
        Ecotx_LostFocus()
        If b = 1 Then
          Ecotx.SetFocus
          Ecotx.Select
          b = 0
        Else
          cdf.SetFocus
          cdf.Select
        Endif

        Stop Event
    End Select
  Endif

  If key.code = key.F1 Then
    Button5_Click()
    Stop Event
  Endif

  If key.code = key.F2 Then
    Select Case Last.tag
      Case 5
        ToggleButton1_click()
        Stop Event

      Case 6
        ToggleButton2_click()
        Stop Event

      Case 7
        ToggleButton3_click()
        Stop Event
    End Select
  Endif

End

Public Sub Btn_KeyPress()

  Select Case Last.tag

    Case 3
      Button1_Click()

  End Select

End

'******************************************************************
'*  Recherche d'un compte vente lors d'une saisie manuelle      *
'******************************************************************
Public Sub compteGman(compt As String)

  Dim Cmpts As Result

  Cmpts = Utils.db.Exec("SELECT * FROM " & Cbase.Table("TabComptes") & "  WHERE compte_cc = &1", compt)
  If Not Cmpts.Available Then
    If son Then
      Music.Play
    Endif
    message.Warning("Ce compte n'existe pas !\n Verifiez votre saisie SVP !")
    b = 1
  Else
    b = 0
  Endif

End

