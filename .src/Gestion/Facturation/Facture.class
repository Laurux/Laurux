Public Function QuitteDate2(dteco As String, impt As String) As String

  Dim dte4 As String

  Tab = "Fiches_Parametres"
  rResult = Utils.db.Exec("SELECT * FROM " & Tab & "")
  dte = rResult!dtepp
  dte2 = Right$(dte, 4) & Left$(dte, 2)
  dte3 = rResult!dtepec
  If rResult!dtepec = "" Then
    Dte3 = Format$(Now, "yyyymm")
  Else
    dte3 = Right$(dte3, 4) & Left$(dte3, 2)
  Endif
  If Right(dte3, 2) = "12" Then
    Mid(dte3, 3, 4) = Str(Val(Right(dte3, 4)) + 1)
    Mid(dte3, 5, 2) = "01"
  Else
    Mid(dte3, 5, 2) = Format(Val(Right(dte3, 2)) + 1, "00")
  Endif
  If rResult!dtepp = "" Then
    Dte2 = Format$(Now, "mmyyyy")
  Else
    Dte2 = rResult!dtepp
    dte2 = Right$(dte, 4) & Left$(dte, 2)
  Endif
  With utils
    dteco = .CDate_calc(dteco)
    dteco = .Cdate_aff(dteco)
    dte4 = rResult!dteclec
    If dteco = "00.00.0:00" Or dteco = ".." Then
      dteco = Format$(Now, "dd.mm.yyyy")
      b = 1
      Editfac = "0"
    Else
      If Bfac.value = True And Not visudoc Then
        If Right$(dteco, 4) & Mid$(dteco, 4, 2) < dte2 Then
          If start.son Then
            Music.Play
          Endif
          If Message.Warning("Attention ! Impression de facture impossible. \nLa date saisie est inférieure à la période précédente. \nVeuillez faire vos clotures SVP !", "Ok") = 1 Then
            Editfac = "0"
            Dtef.SetFocus
            Dtef.Select
            B = "1"
          Endif
        Else
          If Right$(dteco, 4) & Mid$(dteco, 4, 2) > dte3 Then
            If start.son Then
              Music.Play
            Endif
            If Message.Warning("Attention ! Impression de facture impossible. \nLa date saisie est supérieure à la période autorisée. \nVeuillez faire votre cloture mensuelle en facturation SVP", "Ok") = 1 Then
              Editfac = "0"
              Dtef.SetFocus
