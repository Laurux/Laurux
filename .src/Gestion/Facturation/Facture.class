Public Sub Recup_ligbl()

  Dim sline As String
  Dim Lg As Integer = 1
  Dim nlgcar As String
  Dim ob As Object
  Dim i, j, h As Integer = 0
  Dim rescons, reslig As Result

  If $soption = "B" Or $soption = "T" Then i = 2 Else i = 0
  If bTypeFacture Then
    Tab = "Fiches_LigblM"
  Else
    Tab = "Fiches_Ligbl"
  Endif
  Totalmo = "0,00"
  Totalart = "0,00"
  Totalrem = "0,00"
  Totalmottc = "0,00"
  Totalartttc = "0,00"
  Totalremttc = "0,00"
  Totalht = "0,00"
  Totalttc = "0,00"
  Totmo.Text = "0,00"
  Totart.Text = "0,00"
  Totrem.Text = "0,00"
  Totht.Text = "0,00"
  Tottc.Text = "0,00"
  Tottva.Text = "0,00"
  Mrgart = 0
  Mrgmo = 0
  Mrg.text = "0,00"
  Poids.text = "0,000"
  hTxtb.Text = ""
  PJ = ""
  PO = ""
  chkfocus = 1
  Coldetail.Clear
  With utils
    Rligbl = Utils.db.Exec("select * from " & Tab & " where num_ligbl = &1 order by numlig_ligbl", num.text)
    If Rligbl.Available Then
      Repeat
        If Rligbl!typel_ligbl = "C" Then
          If Rligbl!code_ligbl = "XX" Then XX = Rligbl!com_ligbl
          If Rligbl!code_ligbl = "PO" Then PO = Rligbl!com_ligbl
          If Rligbl!code_ligbl = "PJ" And Not IsNull(Rligbl!com_ligbl) Then 
          If IsNull(PJ) Then 
          PJ = Rligbl!com_ligbl
          Else
            Pj &= "\n" & Rligbl!com_ligbl
            Endif
          Endif
          For Each sline In Split(Rligbl!com_ligbl, "\n")
            Coldetail.Add(Rligbl!numlig_ligbl & Lg, Rligbl!numlig_ligbl & Lg)
            Coldetail.item[0] = Rligbl!numlig_ligbl
            Coldetail.item[1] = Rligbl!code_ligbl
            Coldetail.item[2] = sline
            Coldetail.item[9 + i] = Rligbl!typel_ligbl
            Coldetail.item[16 + i] = Rligbl!block_ligbl
            Inc Lg
          Next
        Else
          If Rligbl!typel_ligbl = "R" Then
            Nlgcar = Format$(Rligbl!numlig_ligbl, "0000")
            For Each sline In Split(Rligbl!com_ligbl, "\n")
              Coldetail.Add(Nlgcar & Lg, Nlgcar & Lg)
              Coldetail.item[0] = Nlgcar
              Coldetail.item[1] = Rligbl!code_ligbl
              Coldetail.item[2] = sline
              Coldetail.item[9 + i] = Rligbl!typel_ligbl
              Coldetail.item[16 + i] = Rligbl!block_ligbl
              Inc Lg
            Next
          Else
            If Rligbl!typel_ligbl = "O" Then
              Coldetail.Add(Rligbl!numlig_ligbl, Rligbl!numlig_ligbl)
              Coldetail.item[0] = Rligbl!numlig_ligbl
              Coldetail.item[1] = Rligbl!code_ligbl
              Coldetail.item[2] = Rligbl!libel_ligbl
              Coldetail.item[4 + i] = Rligbl!qte_ligbl
              Coldetail.item[9 + i] = Rligbl!typel_ligbl
              Coldetail.item[16 + i] = Rligbl!block_ligbl
            Else
              Coldetail.Add(Rligbl!numlig_ligbl, Rligbl!numlig_ligbl)
              Coldetail.item[0] = Rligbl!numlig_ligbl
              Coldetail.item[1] = Rligbl!code_ligbl
              If Rligbl!typel_ligbl = "T" Then
                Coldetail.item[2] = Rligbl!libel_ligbl
                Coldetail.item[9 + i] = Rligbl!typel_ligbl
                Coldetail.item[16 + i] = Rligbl!block_ligbl
              Else
                Coldetail.item[2] = Rligbl!libel_ligbl
                If Len(Rligbl!pu_ligbl) - InStr(Rligbl!pu_ligbl, ",") = 2 Then
                  Coldetail.item[3] = Rligbl!pu_ligbl & "  "
                Else
                  Coldetail.item[3] = Rligbl!pu_ligbl
                Endif
                If i <> 0 Then
                  Coldetail.Item[4] = "0"
                  Coldetail.Item[5] = "0"
                  reslig = utils.db.Exec("SELECT * FROM Fiches_ligcons WHERE num=&1 AND numlig=&2 AND type='B'", num.text, num.Text & Rligbl!numlig_ligbl)
                  If reslig.Available Then
                    reslig.MoveFirst
                    For j = 0 To reslig.Max
                      rescons = utils.db.Exec("SELECT * FROM Fiches_consigne WHERE code=&1", reslig!codecons)
                      If rescons.Available Then
                        Select Case rescons!type
                          Case "P"
                            Coldetail.Item[4] = Str(reslig!nb)
                          Case "C"
                            Coldetail.Item[5] = Str(reslig!nb)
                        End Select
                        Coldetail.Item[23 + i] = reslig!envaleur
                      Endif
                      reslig.MoveNext
                    Next
                  Endif
                Endif
                Coldetail.item[4 + i] = Rligbl!qte_ligbl
                If Len(Rligbl!brut_ligbl) - InStr(Rligbl!brut_ligbl, ",") = 2 Then
                  Coldetail.item[5 + i] = Rligbl!brut_ligbl & "  "
                Else
                  Coldetail.item[5 + i] = Rligbl!brut_ligbl
                Endif
                Coldetail.item[6 + i] = Rligbl!rem_ligbl
                If Val(.cpoint(Coldetail.item[6 + i])) <> 0 Then
                  Variables.bRemise = True
                Endif
                If Len(Rligbl!netht_ligbl) - InStr(Rligbl!netht_ligbl, ",") = 2 Then
                  Coldetail.item[7 + i] = Rligbl!netht_ligbl & "  "
                Else
                  Coldetail.item[7 + i] = Rligbl!netht_ligbl
                Endif
                Coldetail.item[9 + i] = Rligbl!typel_ligbl
                If Len(Rligbl!nettc_ligbl) - InStr(Rligbl!nettc_ligbl, ",") = 2 Then
                  Coldetail.item[8 + i] = Rligbl!nettc_ligbl & "  "
                Else
                  Coldetail.item[8 + i] = Rligbl!nettc_ligbl
                Endif
                Coldetail.item[10 + i] = Rligbl!tm_ligbl
                Coldetail.item[12 + i] = Rligbl!tx_ligbl
                Coldetail.item[13 + i] = Rligbl!fam_ligbl
                Coldetail.item[20 + i] = Rligbl!four_ligbl
                Coldetail.item[14 + i] = Rligbl!dec_ligbl
                Coldetail.item[16 + i] = Rligbl!block_ligbl
                Coldetail.item[17 + i] = Rligbl!pdstotal_ligbl
                If Rligbl!typel_ligbl = "A" Then
                  If Not IsNull(Coldetail.item[17 + i]) Then
                    Poids.text = Format$(Val(.cpoint(Poids.text)) + Coldetail.item[17 + i], "0.000")
                  Endif
                  Coldetail.item[18 + i] = Rligbl!mrgart_ligbl
                  Coldetail.item[21 + i] = Rligbl!refl_ligbl
                  Coldetail.Item[22 + i] = Rligbl!tour_ligbl
                  If $soption = "P" Or $soption = "T" Then
                    Coldetail.Item[24 + i] = Rligbl!tipp_ligbl
                  Endif
                  If Not IsNull(Rligbl!mrgart_ligbl) Then Mrgart = Mrgart + Rligbl!mrgart_ligbl
                Endif
                If Rligbl!typel_ligbl = "M" Then
                  Coldetail.item[19 + i] = Rligbl!mrgmo_ligbl
                  If Not IsNull(Rligbl!mrgmo_ligbl) Then Mrgmo = Mrgmo + Rligbl!mrgmo_ligbl
                Endif
                If Not Prxdec Then
                  Mrg.text = Format$(Mrgart + mrgmo, "0.00")
                Else
                  Mrg.text = Format$(Mrgart + mrgmo, "0")
                Endif
                If Rligbl!typel_ligbl <> "E" And Rligbl!typel_ligbl <> "P" And Rligbl!typel_ligbl <> "D" And Rligbl!typel_ligbl <> "T" Then
                  CalculTotal()
                Endif
              Endif
            Endif
          Endif
        Endif
      Until Rligbl.MoveNext()
      If Coldetail.count > 0 Then
        Coldetail.MoveFirst()
        j = 0
        ob = Coldetail.Children[0]
        Repeat
          Try Coldetail.item.Selected = True
          If Not IsNull(Coldetail.item[21 + i]) Then
            For h = 0 To 9
              ob[j, h].Background = &00BFBFFF
            Next
          Else
            For h = 0 To 9
              ob[j, h].Background = Color.TextBackground
            Next
          Endif
          Inc j
        Until Coldetail.MoveNext()
      Endif
      Totmo.Text = Totalmo
      Totart.Text = Totalart
      Totrem.Text = Totalrem
      Totht.Text = Totalht
      Tottc.Text = Totalttc
      Tottva.Text = Format$(Val(.CPoint(Tottc.Text)) - Val(.CPoint(Totht.Text)), "0.00")
      Totht1.Text = Totht.Text
      Totva1.Text = Tottva.Text
      Tottc1.Text = Tottc.Text
      calculfindoc
      If Prxdec Then
        Totmo.Text = Round(Val(Totmo.Text))
        Totart.Text = Round(Val(Totart.Text))
        Totrem.Text = Round(Val(Totrem.Text))
        Totht.Text = Round(Val(Totht.Text))
        Tottc.Text = Round(Val(Tottc.Text))
        Tottva.Text = Round(Val(Tottva.Text))
        Totht1.Text = Round(Val(Totht1.Text))
        Totva1.Text = Round(Val(Totva1.Text))
        Tottc1.Text = Round(Val(Tottc1.Text))
      Endif
    Endif
  End With
  Maj_libelle()
  If Coldetail.count <> 0 Then
    Coldetail.MoveLast()
    Try Coldetail.item.Selected = True
    Nlgart.Text = Format$(Val(Coldetail.current[0]) + 1, "0000")
    Nlg.Text = Format$(Val(Coldetail.current[0]) + 1, "0000")
    NlgCom.Text = Format$(Val(Coldetail.current[0]) + 1, "0000")
    Coldetail.current.EnsureVisible()
  Else
    Nlgart.Text = "0001"
  Endif

End

