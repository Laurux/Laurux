Private Sub creblcom()
  
  Me.mouse = mouse.Wait
  Try Utils.db.Exec("drop TABLE Blcom")
  Utils.db.Exec("CREATE TEMPORARY TABLE Blcom" &
    "(numbl Char(6) NOT NULL," &
    "type Char(1) ," &
    "cdclibl Char(8) ," &
    "nmclibl CHAR(50) ," &
    "pnmclibl CHAR(35) ," &
    "datebl date ," &
    "imp Integer(1), " &
    "numfac Char(10), " &
    "totalttc Decimal(10,2), " &
    "PRIMARY KEY (numbl))" & Utils.db.Engine())
  
End

