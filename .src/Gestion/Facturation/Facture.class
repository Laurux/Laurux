      TextLabel70.Text = "Vendeur connecté"
    Endif
  Endif

  If Key.code = Key.F6 Then
    Select Case Last.tag
      Case 2
        If Start.LocalSettings["/Soc" & Start.Societe & "/Affaire"] Then ChiffreA()
      Case 3
        If Start.LocalSettings["/Soc" & Start.Societe & "/Affaire"] Then ChiffreA()
      Case 4
        If Start.LocalSettings["/Soc" & Start.Societe & "/Affaire"] Then ChiffreA()
    End Select
  Endif

  If key.Control Then
    If Key.code = 65 Then 'CTRL+A comme Aperçu
