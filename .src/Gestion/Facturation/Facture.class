'*****************************************
'*    Actualisation du libellé (casier)  *
'*****************************************
Public Sub Maj_libelle()

  Dim i As Integer

  If bTypeFacture Then
    Tab = "Fiches_LigblM"
  Else
    Tab = "Fiches_Ligbl"
  Endif
  If $soption = "B" Or $soption = "T" Then i = 2 Else i = 0
  If Coldetail.Count <> 0 And Start.LocalSettings["/Soc" & Start.Societe & "/Casier"] <> 0 And Impfac = "0" Then
    Coldetail.MoveFirst
    Repeat
      Coldetail.item.Selected = True
      If Coldetail.current[9 + i] = "A" Then
        rResult = Utils.db.Exec("select * from " & Cbase.Table("TabArt") & " where art_code = &1", Coldetail.item[1])
        If rResult.Available Then
          If InStr(Coldetail.item[2], "~ ") <> 0 Then Try Coldetail.item[2] = Mid$(Coldetail.item[2], InStr(Coldetail.item[2], "~ ") + 2, Len(Coldetail.item[2]))
          If tp = "B" Or If tp = "F" Then
            If Not IsNull(rResult!art_casier) And If rResult!art_stocke And If (Val(Utils.cpoint(rResult!art_qte)) + Val(Utils.cpoint(Coldetail.current[4 + i]))) > 0 Then
              Coldetail.item[2] = rResult!art_casier & "~ " & Coldetail.item[2]
            Else
              If rResult!art_stocke And If (Val(Utils.cpoint(rResult!art_qte)) + Val(Utils.cpoint(Coldetail.current[4 + i]))) <= 0 Then Coldetail.item[2] = "Rupt~ " & Coldetail.item[2]
            Endif
          Endif
          If Tp = "P" Or If tp = "C" Or If tp = "D" Then
            If Not IsNull(rResult!art_casier) And If rResult!art_stocke And If Val(Utils.cpoint(rResult!art_qte)) > 0 Then
              Coldetail.item[2] = rResult!art_casier & "~ " & Coldetail.item[2]
            Else
              If rResult!art_stocke And If Val(Utils.cpoint(rResult!art_qte)) <= 0 Then Coldetail.item[2] = "Rupt~ " & Coldetail.item[2]
            Endif
          Endif
          If Not rResult!art_stocke Then Coldetail.item[2] = "Dep~ " & Coldetail.item[2]
          Try Utils.db.Exec("UPDATE  " & Tab & "  SET  libel_ligbl = &3 WHERE num_ligbl = &1 and numlig_ligbl = &2", num.Text, Coldetail.item[0], Coldetail.item[2])
        Endif
      Endif
    Until Coldetail.Movenext()
  Endif

End

'*****************************************
'* Maj de la quantite en stock en moins  *
'*****************************************
Public Sub Maj_Quantite()

  Dim Qstock As Float
  Dim Qline As Float
  Dim i As Integer

  If $soption = "B" Or $soption = "T" Then i = 2 Else i = 0
  Utils.db.Exec("LOCK TABLES " & Cbase.Table("TabArt") & " WRITE, " & Cbase.Table("TabArtPromo") & " WRITE, " & Cbase.Table("TabLigbl") & " WRITE ")
  With utils
    If Coldetail.Count <> 0 Then
      Coldetail.MoveFirst
      Repeat
        Coldetail.item.Selected = True
        'If Coldetail.Current[9+i] = "C" And Left$(Coldetail.Current[2], 19) = "Pièces en reliquat" Then Break
        If Coldetail.current[9 + i] = "A" Then
          rResult = Utils.db.Exec("select * from " & Cbase.Table("TabArt") & " where art_code = &1", Coldetail.current[1])
          If rResult.Available Then
            Stk = rResult!art_stocke
            If Stk Then
              Qline = Val(.cpoint(Coldetail.current[4 + i]))
              Try Qstock = rResult!art_qte
              If Error Then Qstock = 0
              Try Colvente = rResult!art_colvte
              If Error Then Colvente = 0
              If Colvente > 0 Then Qline = colvente * Qline
              Qstock = Qstock - Qline
              Utils.db.Exec("UPDATE  " & Cbase.Table("TabArt") & "  SET art_qte = &2 WHERE art_code = &1", Coldetail.current[1], Qstock)
              If Promo Then Apromo = ArtPromo.art_Promo(Coldetail.current[1], Cpromo)
              If Apromo = True Then ArtPromo.Maj_qteplus(Coldetail.current[1], Cpromo, Qline)
            Endif
          Endif
        Endif
      Until Coldetail.Movenext()
    Endif
    Qline = 0
    Qstock = 0
  End With
  Utils.db.Exec("UNLOCK TABLES")

End

'**************************************************************************
'* Maj de la quantite en stock en moins lors de la duplication par CTRL+D *
'**************************************************************************
Public Sub Maj_QuantiteD(RefD As String, qted As String)

  Dim Qstock As Float
  Dim Qline As Float

  Utils.db.Exec("LOCK TABLES " & Cbase.Table("TabArt") & " WRITE, " & Cbase.Table("TabArtPromo") & " WRITE, " & Cbase.Table("TabLigbl") & " WRITE ")
  With utils
    If Not IsNull(refd) Then
      rResult = Utils.db.Exec("select * from " & Cbase.Table("TabArt") & " where art_code = &1", refd)
      If rResult.Available Then
        Stk = rResult!art_stocke
        If Stk Then
          Qline = Val(.cpoint(qted))
          Try Qstock = rResult!art_qte
          If Error Then Qstock = 0
          Try Colvente = rResult!art_colvte
          If Error Then Colvente = 0
          If Colvente > 0 Then Qline = colvente * Qline
          Qstock = Qstock - Qline
