Public Sub Colbl_KeyPress()

  If Key.code = Key.enter Or Key.code = Key.return Then ColblSelection(False)

  If key.code = key.F1 Then
    Button7_Click()
    Stop Event
  Endif
  If Key.code = key.Esc Or Key.code = key.f2 Then
    RdimColbl()
    Stop Event
  Endif

End

Public Sub RdimColbl()

  If Not hColbl Then
    colbl.Height = Me.Window.Height / 1.02
    hColbl = True
    Deb1()
  Else
    colbl.Height = Me.Window.Height / 4.70
    hColbl = False
  Endif

End

