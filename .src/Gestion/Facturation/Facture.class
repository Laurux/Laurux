            PdxArt.Visible = True
            TextLabel64.Visible = True
            Poids.Visible = True
          Endif
          If Tp = "B" Or If Tp = "F" And ImpFac <> "1" Then
            'Bsuppr = True
            If Panel8.visible = False Then
              Maj_Quantite2()
            Else
              Maj_QteDep2()
            Endif
            'Bsuppr = False
          Endif
          Button4_Click()
          If Not IsNull(Start.LocalSettings["/Soc" & Start.Societe & "/CodeVendeur"]) And Coldetail.count = 0 Then
            Com.Value = True
            Intitule.text = "Vous avez été servi par " & sVendeur.Text
            Enrg_Ligcom()
          Endif
          If Start.LocalSettings["/Soc" & Start.Societe & "/MoArt"] = True Then
            Motm.Value = True
            Motm_Click()
          Else
            If Start.LocalSettings["/Soc" & Start.Societe & "/ComArt"] = True Then
