Public Sub Ren_ligbl()

  Dim Bloc2 As String
  Dim T As Integer = 1 ' variable pour init ligne
  Dim i As Integer

  If $soption = "B" Or $soption = "T" Then i = 2 Else i = 0
  If Coldetail.Count <> 0 Then
    Coldetail.MoveFirst
    Nlg.Text = "0001"
    Repeat
      Coldetail.item.Selected = True
      If Coldetail.current[9 + i] = "C" Or If TypeL = "R" Then
        Coldetail.current[0] = Nlg.Text
        If Coldetail.current[16 + i] = bloc Then
        Else
          Bloc2 = Nlg.Text
          Bloc = Coldetail.current[16 + i]
          If T <> 1 Then Nlg.Text = Format$(Val(Nlg.Text) + 1, "0000")
          Coldetail.current[0] = Format$(Val(Nlg.Text), "0000")
          T = 0
        Endif
      Else
        If T <> 1 Then
          Nlg.Text = Format$(Val(Nlg.Text) + 1, "0000")
          Coldetail.current[0] = Format$(Val(Nlg.Text), "0000")
        Else
          Coldetail.current[0] = Format$(Val(Nlg.Text), "0000")
        Endif
        T = 0
      Endif
    Until Coldetail.MoveNext()
    If BArt.Value Or If B2art.value Then
      Nlgart.Text = Nlg.Text
      TypeL = "A"
    Endif
    If Motm.value Then
      Nlg.Text = Nlg.Text
      TypeL = "M"
    Endif
    If Com.value Then
      Nlgcom.Text = Nlg.Text
      TypeL = "C"
    Endif
  Else
    Nlg.Text = "0001"
    NlgArt.Text = "0001"
    NlgCom.Text = "0001"
  Endif

End

