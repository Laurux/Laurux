              If snum = "" Then
                res = utils.db.Exec("SELECT numlig FROM Fiches_ligcons WHERE num=&1 and numlig <> 'D' LIMIT 1", Variables.numeroFac)
                If res.Available Then
                  snum = Left(res!numlig, 6)
