'* On enregistre la ligne du numéro de série.
Public Sub Enrg_Numserie()

  Dim maj_serie As Boolean = True
  Dim i As Integer
  Dim int As String

  If $soption = "B" Or $soption = "T" Then i = 2 Else i = 0
  Tab = "Fiches_Ligbl"
  Blocnum = Bloc
  With Utils
    If Modif = "0" Then
      Enrg_Art()
      Coldetail.MoveLast()
      Coldetail.item.Selected = True
      Nlgmodif = Format$(Val(Coldetail.item[0]) + 1, "0000")
      Coldetail.Add(NlgModif, NlgModif)
      Coldetail.item[1] = Numero.Text
      Coldetail.item[2] = "Numéro de série " & Numero.Text
      Coldetail.item[9 + i] = "T"
      Coldetail.item[14 + i] = Dec.text
      Coldetail.item[16 + i] = Bloc
    Else
      Coldetail.MoveFirst()
      Repeat
        int = coldetail.Item.Key
        If Coldetail.item[16 + i] = Blocnum And Coldetail.item[9 + i] = "T" And Left$(Coldetail.item[2], 17) = "Numéro de série" Then
          Coldetail.item[1] = Numero.Text
          Coldetail.item[2] = "Numéro de série " & Numero.Text
          Nlgmodif = Format$(Val(Coldetail.item[0]), "0000")
          maj_serie = False
        Else
          If maj_serie = True And Coldetail.item[0] = Nlgmodif Then
            Enrg_Art()
            If Ecotaxe Or ecotaxe2 Or Taxepv Then coldetail.MoveTo(int)
            Blocnum = Coldetail.item[16 + i]
          Endif
        Endif
      Until Coldetail.MoveNext()
    Endif
    rResult = Utils.db.Exec("SELECT * FROM " & tab & " WHERE num_ligbl = &1 and numlig_ligbl = &2", num.text, NlgModif)
    If rResult.Available Then
      Utils.db.Exec("UPDATE  " & tab & "  SET  num_ligbl = &1 , numlig_ligbl = &2 , libel_ligbl = &3, typel_ligbl = &4, block_ligbl = &5, code_ligbl = &6 WHERE num_ligbl = &1 and numlig_ligbl = &2", numligbl2, NlgModif, "Numéro de série " & Numero.Text, "T", Blocnum, Numero.Text)
    Else
      Utils.db.Exec("INSERT INTO " & tab & "(num_ligbl, numlig_ligbl, libel_ligbl, typel_ligbl, block_ligbl, code_ligbl) VALUES (&1, &2, &3, &4, &5, &6)", numligbl2, NlgModif, "Numéro de série " & Numero.Text, "T", Blocnum, Numero.text)
    Endif
    Enrgnum()
    Majnumserie = 0
    Ren_ligbl()
    Maj_Base()
    Init()
  End With

End

