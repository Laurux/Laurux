Public Sub Envoi()

  Dim sText As String
  Dim sText2 As String
  Dim sCourriel As String
  Dim Ty, modele, immat, serie As String = ""
  Dim tMail, Pjointe As String[]
  Dim sline As String

  Tmail = Split(Courriel.Text, "|")
  Try sCourriel = Replace$(Tmail[1], " ", "")
  If Error Then sCourriel = Courriel.Text
  With Utils
    If Tp = "C" Then sText = "Commande"
    If Tp = "B" Then sText = "Bon"
    If Tp = "D" Then sText = "Devis"
    If Tp = "P" Then sText = "Proforma"
    If Tp = "F" Then sText = "Facture"
    If Start.LocalSettings["/Soc" & Start.Societe & "/AffClient"] Then
      If tp = "C" Or If tp = "D" Or If tp = "P" Then
         If Message.Question("Voulez-vous masquer les coordonées du client ?", "Oui", "Non") = 1 Then
           impdclient = True
         Else
           impdclient = False
        Endif
       Endif
    Endif
    If impdclient Then 
    stext &= " fournisseur "
    Else
      stext &= " client "
    Endif
    If Bfac.value And Visudoc = False Then
      Ty = Nmfac
    Else
      Ty = num.Text
    Endif
    If IsNull(Variables.sTexte) Then
      rResult = Utils.db.Exec("SELECT * FROM " & Cbase.Table("TabTmail") & "")
      If rResult.Available Then
        sText2 = Replace$(rResult!intitule, "**", sText)
        If Not IsNull(sVendeur.Text) Then
          sText2 = Replace$(sText2, "§", sVendeur.Text)
        Else
          sText2 = Replace$(sText2, "§", "")
        Endif
         Else
      Endif
        If Not IsNull(XX) Then sText2 = Stext2 & "\n\n" & XX & "\n\n"
        If Not IsNull(XX) And Not impdclient Then sText2 = Stext2 & "\n\n" & XX & "\n\n"
        Stext2 = "Bonjour,\n\nVeuillez trouver ci-joint votre document No " & ty & " du " & Dtef.Text & "\n\n" & XX & ".\n\nCordialement." & "\n\n"
    Else
      Stext2 = Variables.sTexte & "\n\n" & XX & "\n\n"
      If InStr(variables.sTexte, "**") <> 0 Then Variables.sTexte = Replace$(variables.sTexte, "**", stext)
      If InStr(variables.sTexte, "PO") <> 0 Then Variables.sTexte = Replace$(variables.sTexte, "PO", PO)
      If InStr(variables.sTexte, "XX") <> 0 Then Variables.sTexte = Replace$(variables.sTexte, "XX", XX)
      Stext2 = Variables.sTexte & "\n\n"
    Endif
     sText = sText & " No " & ty & " du " & Dtef.Text
    If Not IsNull(XX) Then
          For Each sline In Split(XX, "\n")
            If InStr(sline, "IMMAT") = 1 Then Try immat = Mid$(sline, InStr(sline, ">") + 1, Len(sline))
            If InStr(sline, "MODELE") = 1 Then Try modele = Mid$(sline, InStr(sline, ">") + 1, Len(sline))
            If InStr(sline, "SERIE") = 1 Then Try serie = Mid$(sline, InStr(sline, ">") + 1, Len(sline))
          Next
           stext &= " " & immat & " " & modele & " " & serie
        Endif
    Visudoc = True
    If Bfac.value And imp = "0" Then
      If start.son Then
        Music.Play
      Endif
      Message.Info("L'envoi d'un document de type Facture ne peut se faire\nque si l'impression a été effectuée !\nVeuillez imprimer votre facture avant de l'envoyer en pièce jointe.")
    Else
      If IsNull(variables.sTexte) Then sText &= Tp & ty
      If Not IsNull(sCourriel) Then
        Impression("Mail")
        Me.mouse = Mouse.Wait
        Wait 1
        If ImpPDF.Value Then
          If IsNull(Start.LocalSettings["/Soc" & Start.Societe & "/Pdfchemin"]) Then
            EnvMail.Envoi2(scourriel, stext, sText2, spiece)
          Else
            EnvMail.Envoi3(scourriel, stext, sText2, spiece)
          Endif
        Else
          If Not IsNull(Colis) Then Colis = Settings["/Soc" & Start.Societe & "/Bexpchemin"] & "/" & Colis
          If Not IsNull(PJ) Then
            Pjointe = Split(PJ, "\n")
          Endif
          If impdclient Then scourriel = ""
          If Not impdclient And Not IsNull(variables.sTexte) Then sText = "Votre " & sText & " No " & ty
          If Not IsNull(PJ) Then
            EnvMail.Envoi2(scourriel, stext, sText2, spiece, "", Pjointe)
         Else
          If Not IsNull(Colis) Then
            EnvMail.Envoi2(scourriel, stext, sText2, spiece, Colis)
          Else
            EnvMail.Envoi2(scourriel, stext, sText2, spiece)
          Endif
          Endif
        Endif
        Wait 1
        Try Me.mouse = Mouse.Default
        'Courriel.Text = sCourriel
      Else
        If start.son Then
          Music.Play
        Endif
        Message.Error("L'adresse mail du client n'a pas été renseignée! Envoi impossible.")
      Endif
    Endif

  End With
  Utils.db.Exec("UNLOCK TABLES")

End

