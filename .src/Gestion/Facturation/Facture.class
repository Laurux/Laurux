  $textpal.Move(x * cw, y, 80 * cw, 24 * ch)
  $textpal.Font = Font["Serif,-1"]
  $textpal.Text = "0"
  $textpal.Alignment = Align.Right

  lab = New Label(panel3)
  lab.text = "Colis"
  lab.Alignment = Align.Center
  lab.Move((x + 120) * cw, y - (15 * ch), 80 * cw, 16 * ch)
  lab.Font = Font["Serif,-1"]
  $textcol = New TextBox(panel3) As "$textcol"
  obs = New Observer($textcol) As "Art"
  $textcol.Move((x + 120) * cw, y, 80 * cw, 24 * ch)
  $textcol.Font = Font["Serif,-1"]
  $textcol.Text = "0"
  $textcol.Alignment = Align.Right

  $checcons = New CheckBox(panel3)
  $checcons.Text = "Consigne"
  $checcons.Move(dec.x + 60 * cw, PdxArt.y, 100 * cw, 24 * ch)
  $checcons.Font = Font["Serif,-1"]
  $checcons.Value = True

End
'*****************gestion du focus palette/colis/qte por consigne*******************

Private Sub foccons()

  If $inbcol <> 0 Then
    $textpal.SetFocus
    $textpal.Select
    Return
  Endif
  If $inbbout <> 0 Then
    $textcol.SetFocus
    $textcol.Select
    Return
  Endif
  Artqte.SetFocus
  Artqte.Select

End

Public Sub $textpal_LostFocus()

  If $textpal.Text Then
    If Not IsNumber($textpal.Text) Then
      ersdec($textpal)
      Return
    Else
      If Int(Val($textpal.Text)) <> Val($textpal.Text) Then
        ersdec($textpal)
        Return
      Endif
      If $inbcol <> 0 Then
        $textcol.Text = Str($inbcol * Val($textpal.Text))
        $inbcol = 0
      Endif
    Endif
  Endif
  $textcol.SetFocus
  $textcol.Select

End

Public Sub $textcol_LostFocus()

  If $textcol.Text Then
    If Not IsNumber($textcol.Text) Then
      ersdec($textcol)
      Return
    Else
      If Int(Val($textcol.Text)) <> Val($textcol.Text) Then
        ersdec($textcol)
        Return
      Endif
      If $inbbout <> 0 Then
        Artqte.Text = Str($inbbout * Val($textcol.Text))
        $inbbout = 0
      Endif
    Endif
  Endif
  Artqte.SetFocus
  Artqte.Select

End

Public Sub $textpal_KeyPress()

  If Key.Code = Key.Enter Or Key.Code = Key.Return Or Key.Code = Key.Tab Then
    Stop Event
    $textpal_LostFocus
  Endif

End

Public Sub $textcol_KeyPress()

  If Key.Code = Key.Enter Or Key.Code = Key.Return Or Key.Code = Key.Tab Then
    Stop Event
    $textcol_LostFocus
  Endif

End

Private Sub ersdec(obj As Object)

  Balloon.Delay = 5000
  Balloon.Warning("<FONT Color=#212121>" & "Zone numérique sans décimale", obj)
  obj.SetFocus

End

Private Sub defpxpp() As String

  Dim res As Result
  Dim pxu As String

  pxu = artpu.Text
  res = utils.db.Exec("SELECT * FROM Fiches_ligtarpp WHERE code=&1 AND binf <= &2 AND bsup > &2", cart.Text, Val(Artqte.Text))
  If res.Available Then
    pxu = Format(res!normht, ar$)
  Endif
  Return pxu

End

