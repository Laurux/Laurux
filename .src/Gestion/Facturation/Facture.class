'recupére la date => ligne bl
Private Function recdate(numbl As String, numlig As String, choix As String) As Date
  
  Dim res As Result
  
  If choix = "B" Then res = utils.db.Exec("SELECT dte_ligbl FROM Fiches_Ligbl WHERE num_ligbl=&1 AND numlig_ligbl=&2", numbl, numlig)
  If choix = "F" Then res = utils.db.Exec("SELECT dtligbl_ligfac as dte_ligbl FROM Fiches_HistoLigfac WHERE num_ligfac=&1 AND numlig_ligfac=&2", numbl, numlig)
  If res.Available Then Return res!dte_ligbl 
  
End


