Public Sub ColblSelection(bright As Boolean)

  Dim bok As Boolean = False
  Dim res As Result

  If bTypeFacture Then
    tab = "Fiches_BlM"
  Else
    tab = "Fiches_Bl"
  Endif
  'Free lock sur doc precedent
  If $numbl <> "" Then
    bok = DocActif.Free_Lock("Facturation", $numbl)
    If bok Then
      Colbl_Deselect()
      Return
    Endif
  Endif
  If colbl.row < 0 Then 'Cas d'une Facture Archive, elle disparait de la liste des BL, aucun BL selectionne
    Colbl_Deselect()
    $numbl = ""
    Return
  Endif
  bencours = False
  CleanChamps()
  If bTypeFacture Then CleanChampsMat()
  dtef.ReadOnly = False
  Dtech.ReadOnly = False
  Nvc = 0
  Panel10.Visible = False
  With utils
    'tentative de prise de lock
    bok = DocActif.Get_Lock("Facturation", colbl[colbl.row, 2].Text)
    If bok Then
      ' On met a jour le numero de BL selectionné
      $numbl = colbl[colbl.row, 2].Text
      ' On récupère les taches associées au bl séléctionné
      If Start.LocalSettings["/Soc" & Start.Societe & "/Taches"] Then
        ColTaches.Columns.count = 5
        ColTaches.Columns[0].Width = 0
        ColTaches.Columns[1].Width = 80
        ColTaches.Columns[2].Width = 120
        ColTaches.Columns[3].Width = 180
        ColTaches.Columns[1].Text = "Documents"
        ColTaches.Columns[2].Text = "Utilisateur"
        ColTaches.Columns[3].Text = "Poste"
        ColTaches.Columns[4].Text = "Libellé de la tâche"
        RecupTaches()
      Endif
      Variables.bRemise = False
      Try rResult = Utils.db.Exec("select * from " & Tab & " where numbl = &1", $numbl)
      If Not Error Then
        'ikey = colbl[colbl.row, 1].Text
        If rResult.count = 0 Then
          rResult = Utils.db.Exec("select * from " & Tab & " where numfac = &1", $numbl)
        Endif
        If rResult.Available Then
          Datej.text = .Cdate_Aff(rResult!datebl)
          If Not IsNull(rResult!ech) Then Dtech.text = .Cdate_Aff(rResult!ech)
          num.text = rResult!numbl
          Code.text = rResult!cdclibl
          Impfac = rResult!imp
          If Impfac = "1" Then
            Code.ReadOnly = True
            Code.Background = &HD4D0C8&
          Else
            Code.ReadOnly = False
            'Code.Background = Color.TextBackground
          Endif

          Cv.Text = rResult!cvclibl
          Nom.text = rResult!nmclibl
          Code3.Text = Code.Text
          num2.Text = num.Text
          Nm.Text = Nom.Text
          If Impfac = "1" Then
            Nom.ReadOnly = True
            Nom.Background = &HD4D0C8&
            Textfrais.ReadOnly = True
            Textfrais.Background = &HD4D0C8&
          Else
            Nom.ReadOnly = False
            Textfrais.ReadOnly = False
            Textfrais.Background = Color.TextBackground
            'Nom.Background = Color.TextBackground
          Endif
          Pnm.Text = rResult!pnmclibl
          If Impfac = "1" Then
            Pnm.ReadOnly = True
            Pnm.Background = &HD4D0C8&
          Else
            Pnm.ReadOnly = False
            'Pnm.Background = Color.TextBackground
          Endif
          Adr1.Text = rResult!adr1bl
          Adr2.text = rResult!adr2bl
          Cp.Text = rResult!cpbl
          Burd.text = rResult!villebl
          res = utils.db.Exec("SELECT * FROM Fiches_Ligbl WHERE num_ligbl=&1 AND typel_ligbl='A' LIMIT 1", $numbl)
          If res.Available Then
            Textto1.Text = res!tour_ligbl
            If $soption = "P" Or $soption = "T" Then
              Texttr1.Text = res!tipp_ligbl
            Endif
          Endif
          Textfrais.Text = "0,00"
          Textfrais.Text = Format(utils.totobs([rResult!gestbl]), "0.00")
          If rResult!acpt = "" Then
            Acpt.Text = "0,00"
          Else
            acpt.Text = Format$(Val(.cpoint(rResult!acpt)), "0.00")
          Endif
          Macompte.Text = acpt.Text
          Try Mrgt.Text = rResult!mreg
          If Error Then Mrgt.Text = "0,00"
          Try Mrgmo = rResult!marge_mo
          If Error Then Mrgmo = 0
          Try Mrgart = rResult!marge_art
          If Error Then Mrgart = 0
          If Not Prxdec Then
            Mrg.text = Format$(Val(.cpoint(Mrgmo)) + Val(.cpoint(Mrgart)), "0.00")
          Else
            Mrg.text = Format$(Val(.cpoint(Mrgmo)) + Val(.cpoint(Mrgart)), "0")
          Endif
          If rResult!rmobl = "" Then
            Txmo.Text = "0,00"
          Else
            Try Txmo.Text = Format$(Val(.cpoint(rResult!rmobl)), "0.00")
            If Error Then Txmo.Text = "0,00"
          Endif
          If rResult!rartbl = "" Then
            Txpieces.Text = "0,00"
          Else
            Try Txpieces.Text = Format$(Val(.cpoint(rResult!rartbl)), "0.00")
            If Error Then Txpieces.Text = "0,00"
          Endif
          Numserie.Text = rResult!numserie
          CodeP.Text = rResult!codep
          Tp = rResult!type
          Numtk = rResult!numtick
          Try Exo.Value = rResult!exobl
