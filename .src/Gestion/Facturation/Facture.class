            Try ArtNet.Text = Format$(Val(.CPoint(Artbrut.Text)) - (Val(.CPoint(Artbrut.Text)) * Val(.CPoint(Artrm.Text)) / 100), Ar$)
            b = 0
            Calcttc()
            Calc_Marge()
          Else
            b = 1
            If start.son Then
              Music.Play
            Endif
            Try message.Warning("Saisie impossible ! Vous avez une remise supérieure 100 %", "OK")
          Endif
        Endif
      Else
        If Val(Artrm.text) <> 0 Then
