  If Start.LocalSettings["/dbase/Con-ssh"] And If Start.LocalSettings["/Soc" & Start.Societe & "/Replique"] And If Start.LocalSettings["/Soc" & Start.Societe & "/Breplique"] Then Btunnel = Utils.ControlTunnel()
  If Btunnel Then
    If $numbl <> "" Then
      bok = DocActif.Free_Lock("Facturation", $numbl)
      If bok Then
        Colbl_Deselect()
        Return
      Endif
    Endif
    CleanChamps()
    If bTypeFacture Then CleanChampsMat()
    modif = "0"
    Code.ReadOnly = False
    Nom.ReadOnly = False
    Pnm.ReadOnly = False
    Tab = "Fiches_Parametres"
    Utils.db.Exec("LOCK TABLES " & Tab & " WRITE, Fiches_Bl WRITE, Fiches_BlM WRITE, Fiches_EntMat WRITE, " & Cbase.Table("TabCli") & " Write, Fiches_Ligbl WRITE, Fiches_LigblM WRITE")
    rResult = Utils.db.Exec("SELECT * FROM " & Tab & " ")
    If rResult.Available Then
      num.Text = rResult!dnbon
    Endif
    If num.Text = "" Then num.Text = "000000"
    num.Text = Val(num.Text) + 1
    num.Text = Format$(num.Text, "000000")
    num2.Text = num.Text
    Datej.Text = Format$(Now, "dd.mm.yyyy")
    rResult = Utils.db.Exec("SELECT * FROM Fiches_Bl where numbl = &1", num.text)
    If Gmateriel Then
      If Not rResult.Available Then rResult = Utils.db.Exec("SELECT * FROM Fiches_BlM where numbl = &1", num.text)
    Endif
    If rResult.available Then
      If start.son Then
        Music.Play
      Endif
      Try Message.Warning("Création impossible ce numéro de BL est déjà attribué")
      Num.text = ""
      Coldetail.Clear
    Else
      Nv = 1
      Nvc = 0
      ' on met a jour les parametres
      Utils.db.Exec("UPDATE  " & Tab & "  SET  dnbon = &1", num.Text)
      datej.SetFocus
      datej.Select
      Coldetail.Clear
      Button8.Visible = False
      Button6.Visible = True
      Impfac = "0"
      'ikey = ""
      'on enregistre le bl en cours
      button4_click()
      'On reinitialise les filtres et affichage BL
      $Desc = False
      sel = ""
      Dbl = "1"
      ComboCom = False
      Titre()
      TriBl = "numbl"
      tribl2 = "numbl"
      Deb1()
      If colbl.Rows.Count > 0 Then
        Colbl.MoveTo(0, 0) ' on se place sur la premiere ligne
        $numbl = num.Text ' Le num BL est logiquement le premier dans le gridview apres reinit des filtres
      Else
        $numbl = num.Text
      Endif
      DocActif.Get_Lock("Facturation", $numbl)
    Endif
    Utils.db.Exec("UNLOCK TABLES")
    If $soption = "B" Or $soption = "T" Then
      $opt.numbl = $numbl
    Endif
    Nv = 1
    If Start.LocalSettings["/Soc" & Start.Societe & "/Factdef"] Then
      BFac.value = True
    Else
      If Start.LocalSettings["/Soc" & Start.Societe & "/Bldef"] Then
        Bbon.value = True
      Else
        If Start.LocalSettings["/Soc" & Start.Societe & "/Devisdef"] Then
          Bdevis.value = True
        Else
          If Start.LocalSettings["/Soc" & Start.Societe & "/Prodef"] Then
            Bprof.value = True
          Endif
        Endif
      Endif
    Endif
    'Nv = 0
    If Start.LocalSettings["/Soc" & Start.Societe & "/Fht"] Then
      sFacht.Value = True
      sTTC.value = True
    Endif
    If Impauto Or If Franchise Then
      Exo.Value = True
      Exo.Enabled = False
    Endif
    TabStrip1[2].Text = "Taches"
    Dep.Enabled = True
    Dep.Text = ""
  Else
    Message.Error("Problème de réplication ! Création de document impossible.")
  Endif

End

