'*   Activation de l'écran d'édition des documents        *
'**********************************************************
Public Sub Bl_Edit()

  Dim Blent As Result
  Dim rParam As Result
  Dim jnal As String
  Dim Vic As String
  Dim Vichq As String
  Dim Viautre As String
  Dim modreg As String
  Dim ech As Variant
  Dim Btunnel As Boolean

  If bTypeFacture Then
    Tab = "Fiches_BlM"
  Else
    Tab = "Fiches_Bl"
  Endif
  Blent = Utils.db.Exec("SELECT cli_reg, cli_expl, cli_cdech, cli_autoent FROM " & Cbase.Table("TabCli") & " where cli_code = &1", code.Text)
  modreg = Blent!cli_reg
  Nbexpl.text = Blent!cli_expl
  ech = Blent!cli_cdech
  Try Impttc.Value = Blent!cli_autoent
  If Error Then Impttc.Value = False
  If IsNull(Nbexpl.text) Then Nbexpl.text = "2"
  Blent = Utils.db.Exec("SELECT * FROM " & Tab & " where numbl = &1", num.Text)
  If Rgmt.value Then
    Regmt.text = Blent!reg
    If IsNull(Regmt.Text) Then Regmt.Text = Modreg
  Else
    Regmt.text = ""
  Endif
  Imp = Blent!imp
  Dfac = Utils.Cdate_Aff(Blent!dtefac)
  Nmfac = Blent!numfac
  rParam = Utils.db.Exec("SELECT * FROM " & Cbase.Table("TabParam") & "")
  If rParam.Available Then
    Jnal = rParam!jrnal2
    Vic = rParam!viremc
    Vichq = rParam!viremchq
    Viautre = rParam!virema
  Endif
  If Jnal = "" Or If Vic = "" Or If Vichq = "" Or If Viautre = "" Then
    If start.son Then
      Music.Play
    Endif
    Message.Error("Vous devez completer, dans les parametres, les données relatives au journal de caisse \net aux comptes de virements internes avant d'imprimer vos documents !")
  Else
    Edit.Visible = True
    Edit.Raise
    Checkdte.Visible = False
    Checkdte.Value = False
    If imp <> 1 Then
      If BFac.Value Or Bbon.Value Or BfacA.Value Then
        Checkdte.Visible = True
      Endif
    Endif
    If Imp <> "1" And BFac.value Then
      If Start.LocalSettings["/dbase/Con-ssh"] And If Start.LocalSettings["/Soc" & Start.Societe & "/Replique"] And If Start.LocalSettings["/Soc" & Start.Societe & "/Breplique"] Then
        Btunnel = Utils.ControlTunnel()
        If Not Btunnel Then
          Message.Error("Le tunnel SSH est out! Impression de facture impossible.")
          Edit.Visible = True
        Endif
      Endif
      Rglt.text = Replace$(Tottc.Text, "-", "")
      If Start.LocalSettings["/Soc" & Start.Societe & "/Rglt"] Then
        Rgmt.Value = True
        Rgmt.Enabled = True
        Rgmt.Visible = True
        HBox6.Visible = True
      Else
        Rgmt.Value = False
        Rgmt.Enabled = False
        Rgmt.Visible = False
        HBox6.Visible = False  
      Endif
      Rglt.ReadOnly = False
      Regmt.Enabled = True
    Else
      If Blent!type <> "F" Then
        Ignore_Remise.Enabled = True
      Else
        Ignore_Remise.Enabled = False
      Endif
      Rglt.Text = Blent!mtreg
      Try Rglt2.Text = Blent!mtreg2
      HBox6.Visible = True
      Rglt.ReadOnly = True
      Regmt.Text = Blent!reg
      Try Regmt2.Text = Blent!reg2
      Regmt.Enabled = False
      Rgmt.Enabled = False
    Endif
    Try Recap.Value = Start.LocalSettings["/Soc" & Start.Societe & "/Recap"]
    Try Impttc.Value = Start.LocalSettings["/Soc" & Start.Societe & "/Impttc"]
    Try Entete.Value = Start.LocalSettings["/Soc" & Start.Societe & "/Entete"]
    Try Reserve.Value = Start.LocalSettings["/Soc" & Start.Societe & "/Conditions"]
    Try Pied.Value = Start.LocalSettings["/Soc" & Start.Societe & "/Pied"]
    Try Imp2L.Value = Start.LocalSettings["/Soc" & Start.Societe & "/Imp2L"]
    Try Coln.value = Start.LocalSettings["/Soc" & Start.Societe & "/Col"]
    Try ChxImp.value = Start.LocalSettings["/Soc" & Start.Societe & "/ChxImp"]
    Try BlA5.Value = Start.LocalSettings["/Soc" & Start.Societe & "/BlA5"]
    Try Impstock.Value = Start.LocalSettings["/Soc" & Start.Societe & "/Impstocke"]
    Try ImpPDF.Value = Start.LocalSettings["/Soc" & Start.Societe & "/ImpPDF"]
    Try Imptotbl.value = Start.LocalSettings["/Soc" & Start.Societe & "/ImpTotbl"]
    'Ignore Remise est une option volatile pour l'instant (pas stocké dans les paramètres, par défault off)
    Ignore_Remise_Click() 'permet de masquer Imptotbl incompatible avec Ignore Remise
    Try Recap.Value = Start.LocalSettings["/Soc" & Start.Societe & "/Recap"]
    Try Cond.Value = Start.LocalSettings["/Soc" & Start.Societe & "/Cond"]
    If Start.LocalSettings["/Soc" & Start.Societe & "/Sms"] Then
      Button30.Visible = True
    Else
      Button30.Visible = False
    Endif
    If Start.LocalSettings["/Soc" & Start.Societe & "/TxtF"] Then
      RegDef.Visible = True
      RegDef.value = True
    Else
      RegDef.Visible = False
      RegDef.value = False
      Regmt.text = Modreg
    Endif
    If Modreg = "Traite" Or If Modreg = "Lcr" Then
      If Left$(Tottc.Text, 1) = "-" Then
        Regmt.text = ""
      Else
        If Imp <> "1" And BFac.value Then
          Regmt.text = Modreg
          ' Rglt.Text = Replace$(Tottc.Text, "-", "")
          Rgmt.value = True
          Label6.Visible = True
          Rglt.Visible = True
        Endif
      Endif
    Endif
    If Impauto Or If Franchise Then
      Impttc.Enabled = False
      Impttc.value = False
    Endif
    If Not Error Then
      If Imp = "1" Then
        dtef.Text = Utils.Cdate_Aff(Blent!dtefac)
        dtef.ReadOnly = True
        Dtech.ReadOnly = True
        Regmt.text = Blent!reg
      Else
        dtef.Text = Format$(Now, "dd.mm.yyyy")
        dtef.ReadOnly = False
        Dtech.ReadOnly = False
      Endif
      If Imp = "1" Then
        Dtech.text = Utils.Cdate_Aff(Blent!ech)
      Else
        Dtech.text = dateEcheance(Ech)
      Endif
      If Dtech.text = ".." Then Dtech.text = Dtef.Text
      If Blent!type <> "F" Then
        AffBledit2.visible = True
        'Aff_AdrC()
         Affadrliv()
        If Colal.Count > 0 Then
          AffBledit2.Value = True
          Colal_Click()
          Edt_gotfocus()
        Endif
      Else
        AffBledit2.visible = False
      Endif
    Else
      If start.son Then
        Music.Play
      Endif
      Try Message.Warning("Veuillez completer vos références SVP !", "OK")
    Endif
  Endif
Catch
  If start.son Then
    Music.Play
  Endif
  Try message.Error(Error.Text & " " & Error.where)

End

