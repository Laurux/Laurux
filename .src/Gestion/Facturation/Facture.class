'**********************************************************
'*                    Suppression Bon                     *
'**********************************************************
Public Sub Bl_Suppr()

  Dim Blrslt As Result
  Dim Bannul As Boolean = True

  If Not bTypeFacture Then
    Tab2 = "Fiches_Ligbl"
    Tab = "Fiches_Bl"
  Else
    Tab2 = "Fiches_LigblM"
    Tab = "Fiches_BlM"
  Endif
  If Not IsNull(num.text)
    If start.son Then
      Music.Play
    Endif
    If Not bTypeFacture Then
      Blrslt = Utils.db.Exec("SELECT * FROM " & tab2 & " where num_ligbl = &1 and typel_ligbl = &2 and left(libel_ligbl,9) = &3", num.Text, "T", "Numero de")
      If Blrslt.Available Then Bannul = False
    Endif
    If Bannul Then
      Blrslt = Utils.db.Exec("SELECT * FROM " & tab & " where numbl = &1", num.Text)
      If Blrslt.Count > 0 Then
        If (Blrslt!imp = "1") Or (Blrslt!numtick <> Null) Then
          If start.son Then
            Music.Play
          Endif
          If (Blrslt!imp = "1")
            Try Message.Warning("Suppression impossible, la facture a été imprimée !")
          Else
            Try Message.Warning("Suppression impossible, le bon est règlé en caisse... attente de moulinage!")
          Endif
        Else
          If start.son Then
            Music.Play
          Endif
          If Message.Question("Etes-vous sur de vouloir supprimer ce bon ?", "Oui", "Non") = 1 Then
            DocActif.Free_Lock("Facturation", $numbl)
            Utils.db.Exec("DELETE FROM " & tab & " where numbl = &1", $numbl)
            Utils.db.Exec("DELETE FROM " & tab2 & " where num_ligbl = &1", $numbl)
            If Gmateriel Then
              'On supprime le matériel selectionné et on le replace en non vendu
              Utils.db.Exec("Update " & Tmat & " set mat_vendu = &2, mat_bl = &3, mat_cli = &4, mat_qte = &5 where mat_bl = &1", $numbl, 0, "", "", 1)
            Endif
            If $soption = "B" Or $soption = "T" Then
              utils.db.Exec("DELETE FROM Fiches_ligcons WHERE num=&1 AND type='B'", num.Text)
              $opt.affcons(num.Text)
              $opt.affdecons
              $opt.affdroit(num.Text)
            Endif
            If Tp = "B" Or Tp = "F" Then
              If Panel8.visible = False Then
                'On met à jour le stock du produit
                Maj_Quantite2()
              Else
                'On met à jour le stock du depot
                Maj_QteDep2()
              Endif
            Endif
            Colbl_Deselect()
            CleanChamps()
            If bTypeFacture Then CleanChampsMat()
            Coldetail.Clear
            Form_Open()
          Endif
        Endif
        code.Background = Color.TextBackground
        Button8.Visible = False
        Button6.Visible = True
      Endif
    Else
      Try Message.Warning("Suppression impossible ! Veuillez supprimer vos lignes de matériels avant de supprimer le BL SVP.")
    Endif
  Endif

End

