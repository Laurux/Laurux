    End Select
  Endif

  If Key.code = Key.F5 Then
    ConVendeurs.ShowModal()
    If Start.LocalSettings["/Soc" & Start.Societe & "/CodeVendeur"] = "" Then
      sVendeur.Text = ""
      TextLabel70.Text = ""
    Else
