  If Key.code = Key.Escape Or Key.code = Key.F2 Then
    If Colcli.Visible = True Then
      ColCli.visible = False
      ColCli.clear
      Client1.SetFocus
      Client1.Select
      Stop Event
    Endif
  Endif

End

