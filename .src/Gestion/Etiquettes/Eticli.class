Public Sub colinit(Col As GridView, Coln As String)

  With Utils
    If sens = "Asc" Then
      sens = "Desc"
    Else
      sens = "Asc"
    Endif
    If coln = 0 Then
      tri = "cli_code"
      AffClients(tri, sens, Col)
    Endif
    If coln = 1 Then
      tri = "cli_nom"
      AffClients(tri, sens, Col)
    Endif
  End With

End

