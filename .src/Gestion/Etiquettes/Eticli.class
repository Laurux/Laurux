Public Sub _New()
  
  Dim Frmt As New String[]
  
  Me.Center
  
  Music.Load(Start.Musique)
  Frmt = Utils.FColr(Settings["/Coul/Znaff"])
  CoulZnaf = Val(Frmt[0])
  Frmt = Utils.FColr(Settings["/Coul/Focus"])
  Coulfoc = Val(Frmt[0])
  Frmt = Utils.FColr(Settings["/Coul/Znss"])
  CoulZns = Val(Frmt[0])
  If Settings["/Soc" & Start.Societe & "/Coul_fen"] Then Utils.Observers(Me)
  If CheckType.value = False Then
    Radiobutton2.Visible = False
    Panel3.Visible = False
  Endif
  If CheckCp.value = False Then
    Radiobutton3.Visible = False
    Panel2.Visible = False
  Endif
  Type.Text = "00000"
  Type2.Text = "ZZZZZ"
  client1.Text = "41100001"
  client2.Text = "4119999999"
  Cp1.text = "01000"
  Cp2.Text = "94738"
  client1.Select
  client1.SetFocus
  SetObservers(Me, Me)
  Utils.Observers(Me)
  init_ComboBox()
  
End

Public Sub init_ComboBox()
  
  Dim retiq As Result
  
  Codetiq.clear
  Try rEtiq = Utils.db.Exec("SELECT * FROM " & Tab & " ")
  If Not Error Then
    If rEtiq.Available Then
      Do
        Codetiq.Add(rEtiq!code & "- " & rEtiq!libelle)
      Loop Until rEtiq.MoveNext()
    Endif
    Codetiq.Text = ""
  Endif
  
End

