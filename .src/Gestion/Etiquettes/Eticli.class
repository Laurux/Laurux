Public Sub ColCli_KeyPress()
  
  If key.code = key.F1 Then
    Button1_Click()
    Stop Event
  Endif
  
  If Key.code = Key.Enter Or Key.code = Key.Return Then
    ColCli_Click()
  Endif
  
  If Key.code = Key.Escape Or Key.code = Key.F2 Then
    If Colcli.Visible = True Then
      ColCli.visible = False
      ColCli.clear
      Client1.SetFocus
      Client1.Select
      Stop Event
    Endif
  Endif
  
End

