Public Sub SelectionArt()

  Dim Tab As String = ""
  Dim MyForm As TriArticles

  Tab = "Fiches_Art"
  sel = ""
  Tri = "art_code"
  MyForm = New TriArticles(True, "", Tri, "Etigond", "", "")
  MyForm.Showmodal()
  If Variables.Bsel = True Then
    b = 0
    If Origine = "1" Then
      Art.text = Variables.Codearticle
      If Liste.value = True Then
        Art_man()
      Else
        Art2.SetFocus
        Art2.Select
      Endif
    Else
