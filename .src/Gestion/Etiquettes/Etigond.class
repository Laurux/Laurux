Public Sub Gridbart_Update()

  Dim i As Integer

  Tab2 = "Etiquettes"
  Gridbart.Clear
  Gridbart.Rows.Count = 4
  i = 0
  Rarts = Utils.db.Exec("SELECT * FROM " & Tab2 & " order by code")
  If Rarts.Available Then
    Gridbart.Rows.Count = Rarts.count
    Repeat
      Rarts2 = Utils.db.Exec("SELECT * FROM " & Cbase.Table("TabArt") & " where art_code = &1", Rarts!code)
      Gridbart[i, 0].Text = Rarts!code
      If Rarts2.Available Then Gridbart[i, 1].Text = Rarts2!art_design
      Gridbart[i, 2].Text = Rarts!nombre
      i += 1
    Until Rarts.MoveNext()
  Endif

End

Public Sub Gridbart_Click()

  Dim Code As String

  Tab2 = "Etiquettes"
  Code = Gridbart[Gridbart.Row, 0].Text
  Rarts2 = Utils.db.Exec("SELECT * FROM " & Tab2 & " where code = &1", Code)
  If Rarts2.Available Then
    Select Message.Question("Que voulez-vous faire ?", "Cumuler", "Remplacer", "Supprimer")
      Case 1
        Nbre = Rarts2!nombre + Val(Nbtiq.text)
        Utils.db.Exec("UPdate  " & Tab2 & "  SET  code = &1 , nombre = &2 WHERE code = &1", Code, Nbre)
      Case 2
        Nbre = Val(Nbtiq.text)
        Utils.db.Exec("UPdate  " & Tab2 & "  SET  code = &1 , nombre = &2 WHERE code = &1", Code, Nbre)
      Case 3
        Utils.db.Exec("Delete From " & Tab2 & "  where  code = &1", Code)
    End Select
  Endif

  Gridbart_Update()

End

