Public Sub Button2_Click()

  Dim dte As String
  Dim i As Integer

  Try Kill User.Home & "/tmp/EtiqGond.pdf"
  If IsNull(codetiq.text) Then
    Message.Warning("Veuillez selectionner un modèle d'étiquettes SVP !")
    Return
  Endif

  i = 1
  i2 = 0
  Nbetiq = 1
  code = ""
  Shell "cd " & User.Home & ""
  Filename = User.Home & "/tmp/EtiqGond.pdf"
  Init()
  Try Kill Filename
  If A4.Value Then
    pdf = New CEtiquettes(PageA[1], "mm", PageA[0])
  Else
    If Dymo.Value Then
      pdf = New CEtiquettes("Portrait", "mm", "dymog")
      'On devrait plutot retrouver ces parametres dans la base, voir a remonter la taille depuis la classe PDFWriter
      Hauteur = 35.0
      Largeur = 88.0
      Mrghaut = 0
      Mrgauche = 0
      txtPosX = 2
      txtPosY = 0
    Endif
  Endif
  pdf.Open()
  pdf.NewPage()
  SorP = False
  dte = Format$(Now, "dd.mm.yyyy")
  Tab2 = "Fiches_Tvaav"
  Tab3 = "Etiquettes"
  Me.Mouse = Mouse.Wait

  If Liste.Value = False Then
    If Radiobutton1.Value Then
      rarts = Utils.db.Exec("SELECT * FROM " & Cbase.Table("TabArt") & " WHERE art_code >= &1  and art_code <= &2 and art_fam >= &3 and art_fam <= &4 and art_four >= &5 and art_four <= &6 order by CAST(art_code AS char)", Art.Text, Art2.Text, Fam.Text, Fam2.Text, four.Text, four2.Text)
    Else
      If Radiobutton2.Value Then
        rarts = Utils.db.Exec("SELECT * FROM " & Cbase.Table("TabArt") & " WHERE art_code >= &1  and art_code <= &2 and art_fam >= &3 and art_fam <= &4 and art_four >= &5 and art_four <= &6 order by art_fam, art_code", Art.Text, Art2.Text, Fam.Text, Fam2.Text, four.Text, four2.Text)
      Else
        If Radiobutton3.Value Then
          rarts = Utils.db.Exec("SELECT * FROM " & Cbase.Table("TabArt") & " WHERE art_code >= &1  and art_code <= &2 and art_fam >= &3 and art_fam <= &4 and art_four >= &5 and art_four <= &6 order by art_four, art_code", Art.Text, Art2.Text, Fam.Text, Fam2.Text, four.Text, four2.Text)
        Else
          If Radiobutton4.Value Then
            rarts = Utils.db.Exec("SELECT * FROM " & Cbase.Table("TabArt") & " WHERE art_code >= &1  and art_code <= &2 and art_fam >= &3 and art_fam <= &4 and art_four >= &5 and art_four <= &6 order by art_design, art_code", Art.Text, Art2.Text, Fam.Text, Fam2.Text, four.Text, four2.Text)
          Else
            If Radiobutton7.Value Then
              rarts = Utils.db.Exec("SELECT * FROM " & Cbase.Table("TabArt") & " WHERE art_code >= &1  and art_code <= &2 and art_fam >= &3 and art_fam <= &4 and art_four >= &5 and art_four <= &6 order by art_casier, art_code", Art.Text, Art2.Text, Fam.Text, Fam2.Text, four.Text, four2.Text)
            Endif
          Endif
        Endif
      Endif
    Endif
  Endif
  If Liste.Value = True Then
    rarts2 = Utils.db.Exec("SELECT * FROM " & Tab3 & "")
    Repeat
      Try rarts = Utils.db.Exec("SELECT * FROM " & Cbase.Table("TabArt") & " WHERE art_code = &1", rarts2!code)
      If Not Error Then Imprime(nb)
    Until rarts2.MoveNext()
    fin_impression()
  Else
    If rarts.Available Then
      Repeat
        Imprime(nb)
      Until rarts.MoveNext()
      fin_impression()
    Else
      If son Then
        Music.Play
      Endif
      message(" Aucun article pour cette demande", "OK")
    Endif
  Endif

  Me.Mouse = Mouse.Pointing
  txtPosX = 0
  txtPosY = 0

End

Public Sub fin_impression()

  pdf.Output(Filename, False)
  Visualiseur.Prog_Editeur(Filename)
  If son Then
    Music.Play
  Endif
  message(" Traitement terminé.", "OK")
  Gridbart.Clear
  Gridbart.Rows.Count = 4
  liste_Click()

End

' on imprime
Public Sub Imprime(nb As Boolean)

  Dim rTvav As Result
  Dim Fami As String
  Dim fourn As String
  Dim cfourn As String
  Dim pvht As String
  Dim tva As String
  Dim pvttc As String
  Dim nbdec As String
  Dim nbd As String
  Dim volm As String
  Dim unite As String
  Dim i As Integer
  Dim Casier As String
  Dim barcode As String
  Dim FontRatio As Float

  volm = ""
  With Utils
    If Liste.Value = True Then
      nombretiq = rarts2!nombre
    Else
      nombretiq = Val(Nbtiq.Text)
    Endif
    code = rarts!art_code
    intitule = rarts!art_design
    intitule2 = rarts!art_design2
    barcode = rarts!art_cbarre
    fami = rarts!art_fam
    fourn = rarts!art_four
    cfourn = rarts!art_cfour
    casier = rarts!art_casier
    nbd = rarts!art_nbd
    nbdec = Utils.Find_nbdec(nbd)
    pvht = Format$(rarts!art_pvht, nbdec)
    unite = rarts!art_vol
    If Len(pvht) - InStr(pvht, ",") = 2 Then pvht = pvht & " "
    tva = rarts!art_tva
    rTvav = Utils.db.Exec("SELECT * FROM " & Cbase.Table("TabTvav") & " WHERE code_tva = &1", tva)
    tva = rTvav!taux_tva & " %"
    pvttc = Format$(rarts!art_pvttc, nbdec)
    If Len(pvttc) - InStr(pvttc, ",") = 2 Then pvttc = pvttc & " "
    'On imprime
    If EtiqStk.value = True Then
      If rarts!art_stocke And Val(Utils.cpoint(rarts!art_qte)) >= 1 Then
        nombretiq = Val(Utils.cpoint(rarts!art_qte))
      Else
        Return
      Endif
    Endif
    If RadioButton5.value = True Then Try volm = Format$(Val(Utils.cpoint(pvht)) / Val(Utils.cpoint(rarts!art_poids)), nbdec)
    If RadioButton6.value = True Then Try volm = Format$(Val(.cpoint(pvttc)) / Val(.cpoint(rarts!art_poids)), nbdec)
    If IsNull(volm) Then volm = "0"
    If A4.Value And InStr(Codetiq.text, "Gondole") <> 0 Or If Dymo.value Then
      For i = 1 To nombretiq
        If nb = False Then pos_Etiq()
        FontRatio = Largeur / 100.0
        'pdf.Rectangle2(txtPosX, txtPosY, Largeur, Hauteur)
        pdf.Code(code, txtPosX, txtPosY, FontRatio)
        pdf.IntituleA(Intitule, txtPosX, txtPosY + (3 * FontRatio), FontRatio)
        If CheckBox4.Value Then pdf.Refour(fourn, txtPosX, txtPosY + (6 * FontRatio), FontRatio)
        If CheckBox3.Value Then pdf.Refour(cfourn, txtPosX + ((Largeur / 2) + 1), txtPosY + (6 * FontRatio), FontRatio)
        If (Not CheckBox4.Value) And (Not CheckBox3.Value) Then
          pdf.IntituleA(Intitule2, txtPosX, txtPosY + (6 * FontRatio), FontRatio)
        Endif
        If Not IsNull(Barcode) And If Len(Barcode) = 13 Then
          pdf.Barcode(Barcode, txtPosX + 2, txtPosY + ((Hauteur / 2) - (Hauteur / 6)), (Hauteur / 3), (Largeur - 6), FontRatio)
        Endif
        pdf.Tva(Tva, txtPosX + 2, txtPosY + (Hauteur - (12 * FontRatio)), FontRatio)

        'If RadioButton5.Value = True And Not IsNull(volm) Then pdf.volm(volm, unite, 1, txtPosX + 28, txtPosY + 14)
        If RadioButton5.Value = True And Val(Utils.cpoint(Volm)) > 0 Then
          pdf.volm(Volm, unite, 1, txtPosX + ((Largeur / 2) + 1), txtPosY + (Hauteur - (9 * FontRatio)), FontRatio)
        Endif
        If RadioButton6.Value = True And Val(Utils.cpoint(Volm)) > 0 Then
          pdf.volm(Volm, unite, 0, txtPosX + ((Largeur / 2) + 1), txtPosY + (Hauteur - (9 * FontRatio)), FontRatio)
        Endif

        pdf.Rectangle2(txtPosX + 1, txtPosY + (Hauteur - (8 * FontRatio)), (Largeur - 2), (7 * FontRatio))
        If CheckBox1.Value Then pdf.PrixHt(Pvht, txtPosX + 2, txtPosY + (Hauteur - (7 * FontRatio)), FontRatio)
        If CheckBox2.Value Then pdf.PrixTTC(Pvttc, txtPosX + ((Largeur / 2) + 1), txtPosY + (Hauteur - (7 * FontRatio)), FontRatio)
        Inc Nbetiq
        If nb = True Then
          pos_Etiq()
          If i = nombretiq Then nb = False
        Endif
      Next
    Endif
    If A4.Value And InStr(Codetiq.text, "Article") <> 0 Then
      For i = 1 To nombretiq
        If nb = False Then pos_Etiq()
        'pdf.Rectangle2(txtPosX, txtPosY, Largeur, Hauteur)
        'pdf.Code(code, txtPosX, txtPosY)
        FontRatio = Largeur / 100.0
        pdf.IntituleB(Intitule, txtPosX, txtPosY, FontRatio)
        pdf.Tva(Tva, txtPosX, txtPosY + (14 * FontRatio), FontRatio)
        If RadioButton5.Value = True And Val(Utils.cpoint(Volm)) > 0 Then
          pdf.Poids(rarts!art_poids, unite, txtPosX, txtPosY + (5 * FontRatio), FontRatio)
          pdf.volm(Volm, unite, 1, txtPosX + ((Largeur / 2) + 1), txtPosY + (5 * FontRatio), FontRatio)
        Endif
        If RadioButton6.Value = True And Val(Utils.cpoint(Volm)) > 0 Then
          pdf.Poids(rarts!art_poids, unite, txtPosX, txtPosY + (5 * FontRatio), FontRatio)
          pdf.volm(Volm, unite, 0, txtPosX + ((Largeur / 2) + 1), txtPosY + (5 * FontRatio), FontRatio)
        Endif
        pdf.PrixTTCB(Pvttc, txtPosX + Largeur - (Largeur / 4), txtPosY + (8 * FontRatio), FontRatio)
        If Not IsNull(Barcode) And If Len(Barcode) = 13 Then
          pdf.Barcode(Barcode, txtPosX + 2, txtPosY + (Hauteur - (Hauteur / 3)), (Hauteur / 3), (Largeur - 6), FontRatio)
        Endif
        If CheckBox4.Value Then pdf.Refour(fourn, txtPosX, txtPosY + (17 * FontRatio), FontRatio)
        If CheckBox3.Value Then pdf.Refour(cfourn, txtPosX + ((Largeur / 2) + 1), txtPosY + (17 * FontRatio), FontRatio)
        Inc Nbetiq
        If nb = True Then
          pos_Etiq()
          If i = nombretiq Then nb = False
        Endif
      Next
    Endif
    If ConnZbr = True Then
      Volm = Volm & " Kg/l"
      For i = 1 To nombretiq
        If CheckBox1.Value Then
          Zebra.Gondole1(MySock, Barcode, pvht, intitule, Code, "HT", Casier, volm, Cfourn)
        Else
          Zebra.Gondole1(MySock, Barcode, pvttc, intitule, Code, "TTC", casier, volm, Cfourn)
        Endif
      Next
    Endif
  End With

End

Public Sub pos_Etiq()

  If A4.Value Then
    If Nbetiq = 1 Then
      txtPosY = Mrghaut
      txtPosX = 2 + Mrgauche
    Else
      txtPosX = 2 + Mrgauche + ((Largeur + Esplargeur) * (Nbetiq - 1))
    Endif
    If Nbetiq > Val(Nblargeur) Then
      Nbetiq = 1
      txtPosX = 2 + Mrgauche
      txtPosY = txtPosY + Hauteur + Esphauteur
      Inc i2
      If i2 >= Val(Nbhauteur) Then
        pdf.NewPage()
        i2 = 0
        Nbetiq = 1
        txtPosX = 2 + Mrgauche
        txtPosY = Mrghaut
      Endif
    Endif
  Else
    If Nbetiq > 1 Then
      pdf.NewPage()
      i2 = 0
      Nbetiq = 1
      txtPosX = 2 + Mrgauche
      txtPosY = Mrghaut
    Endif
  Endif

End

Public Sub Connect()

  MySock = New Socket
  Try MySock.Connect(IpZebra, 9100)
  If Not Error Then
    Do While (MySock.Status <> 7) And (MySock.Status > 0)
      Wait 0.1
    Loop

    If MySock.Status <> 7 Then
      Message.Error("Connexion impossible de l'imprimante Zebra a l'IP " & IpZebra)
      ConnZbr = False
    Else
      ConnZbr = True
    End If
  Else
    Message.Warning("L'adresse " & IpZebra & " n'existe pas dans votre réseau ! Connexion impossible.")
  Endif

End

