  Gridbart.Columns[0].Title = "code"
  Gridbart.Columns[0].Width = 125
  Gridbart.Columns[1].Title = "intitule"
  Gridbart.Columns[1].Expand = True
  Gridbart.Columns[2].Title = "quantite"
  Gridbart.Columns[2].Width = 50
