Public Sub Btn_Click()
  
  Select Case Last.tag
      
    Case 1
      Button1_Click()
      
    Case 2
      Button2_Click()
      
    Case 3
      Try Kill User.home & "/EtiqArt.ps"
      Me.Close
      
    Case 4
      If IsNull(codetiq.text) Then
        Message.Warning("Veuillez selectionner un modèle d'étiquettes SVP !")
        Return
      Endif
      If message.Question("Attention ! Le fichier 'inventaire.txt' du portable doit être vidé dans le repertoire " & "~/Portable !") = 1 Then
        Liste.Value = True
        Import_Portable()
      Endif
      
  End Select
  
End

Public Sub Btn_KeyPress()
  
  Select Case Last.tag
    Case 2
      Button2_Click()
  End Select
  
End

Public Sub Cmpt_KeyPress()
  
  If Key.code = Key.enter Or Key.code = Key.return Or Key.code = Key.Tab Then
    Select Case Last.tag
      Case 1
        If Liste.Value = False Then
          Art_man()
          Art2.SetFocus
          Art2.Select
          Stop Event
        Else
          Art_man()
        Endif
        
      Case 2
        fam.SetFocus
        fam.Select
        Stop Event
        
      Case 3
        fam2.SetFocus
        fam2.Select
        Stop Event
        
      Case 4
        four.SetFocus
        four.Select
        Stop Event
        
      Case 5
        four2.SetFocus
        four2.Select
        Stop Event
        
      Case 6
        Nbtiq.SetFocus
        Nbtiq.Select
        Stop Event
        
      Case 7
        Posetiq.SetFocus
        Posetiq.Select
        Stop Event
        
      Case 8
        Button2.SetFocus
        Stop Event
        
    End Select
  Else
    Select Case Last.Tag
      Case 7
        ChkInput()
        
      Case 8
        ChkInput()
    End Select
  Endif
  
  If key.code = key.F1 Then
    Button1_Click()
    Stop Event
  Endif
  
  If key.code = key.F2 Then
    Select Case Last.tag
      Case 1
        ToggleButton1_Click()
        Stop Event
      Case 3
        ToggleButton3_Click()
        Stop Event
      Case 4
        ToggleButton4_Click()
        Stop Event
      Case 5
        ToggleButton5_Click()
        Stop Event
      Case 6
        ToggleButton6_Click()
        Stop Event
    End Select
  Endif
  
End

