Public Sub colarts_Data(Row As Integer, Column As Integer)
  
  Dim Col As Integer
  Dim Frmt As String[]
  Dim sFormat As String
  
  With Utils
    ArtTable[0] = "art_code"
    ArtTable[1] = "art_design"
    ArtTable[2] = "art_fam"
    'ArtTable[3] = "art_cequ"
    If tri = "art_code" Then
      ArtTable[3] = "art_cequ"
    Else
      If tri = "art_design" Then
        ArtTable[4] = "art_cequ"
      Else
        If tri = "art_fam" Then
          ArtTable[3] = "art_cequ"
        Else
          ArtTable[3] = Tri
        Endif
      Endif
    Endif
    Try .rs1.MoveTo(Row)
    Try colarts.data.Text = Str(.rs1[Arttable[Column]])
    
  End With
  
End

