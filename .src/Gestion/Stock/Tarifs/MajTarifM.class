'****************************************************
'*      Gestion du focus. Routine de Charlie Reinl  *
'****************************************************
Public Sub Cmpt_GotFocus()
  
  Utils.SetEditColor(Me, Last)
  Last.SELECT
  Select Case Last.tag
      
    Case 9
      If Val(Utils.cpoint(Rem1.text)) > 0 Then Panel6.visible = True
      Stop Event
      
    Case 10
      If b = 0 Then PaUAGF2()
      Stop Event
      
    Case 11
      If b = 0 Then Txc = Txconv2.Text
      Stop Event
  End Select
  b = 0
  
End

Public Sub Cmpt_Change()
  
  Select Case Last.tag
    Case 16
      If Checkcmpt Then bModif = True
  End Select
  
End

'************************************************
'* On appelle la liste des fournisseurs         *
'************************************************
Public Sub Button4_Click()
  
  Dim Rfour As Result
  
  If fourlist.visible Then
    fourlist.clear
    fourlist.visible = False
  Else
    fourlist.visible = True
    fourlist.Columns.count = 2
    fourlist.Columns[0].Width = 65
    fourlist.Columns[1].Width = 180
    fourlist.Columns[0].Text = "code"
    fourlist.Columns[1].Text = "Intitulé"
    Rfour = Utils.db.Exec("SELECT * FROM " & Cbase.Table("TabFour") & " ")
    Do
      fourlist.Add(Rfour!fo_code, Rfour!fo_code)
      fourlist.Item[0] = Rfour!fo_code
      fourlist.Item[1] = Rfour!fo_nom
    Loop Until Rfour.MoveNext()
    fourList.MoveFirst
    fourList.SetFocus
    fourList.Item.Selected = True
  Endif
  
End

'**************************************************
'* Selection d'un fournisseur a la souris         *
'**************************************************
Public Sub fourList_Click()
  
  four.text = fourList.Item[0]
  fournom.text = fourList.Item[1]
  fourList.clear
  fourList.visible = False
  
End

'***************************************************
'* Selection d'un fournisseur manuellement         *
'***************************************************
Public Sub fourList_KeyPress()
  
  If Key.code = Key.Enter Or Key.code = Key.Return Then
    fourList_Click()
  Endif
  If Key.code = Key.Escape Or Key.code = Key.F2 Then
    fourList.visible = False
    fourList.clear
    four.SetFocus
    Stop Event
  Endif
  
End

'******************************************** Début de la gestion des zones de prix *************************************

Public Sub Cmpt_KeyPress()
  
  If Key.code = Key.enter Or Key.code = Key.return Or Key.code = Key.Tab Then
    Select Case Last.tag
      Case 8
        pbhtLF2()
        If b = 1 Then
          pbht2.SetFocus
          pbht2.Select
        Else
          Tr2.SetFocus
          Tr2.Select
        Endif
        b = 0
        Stop Event
        
      Case 9
        TrC2()
        If b = 1 Then
          Tr2.SetFocus
          Tr2.Select
        Else
          If Val(Utils.cpoint(Rem4.text)) > 0 Then
            Rem4.SetFocus
            Rem4.Select
          Else
            PaUAGF2()
            PAua2.SetFocus
            PAua2.Select
          Endif
          Chkfocus = 0
        Endif
        b = 0
        Stop Event
        
      Case 10
        PaUALF2()
        If b = 1 Then
          PAua2.SetFocus
          PAua2.Select
        Else
          Txconv2.SetFocus
          Txconv2.Select
          Chkfocus = 0
        Endif
        b = 0
        Stop Event
        
      Case 11
        TxLF2()
        If b = 1 Then
          Txconv2.SetFocus
          Txconv2.Select
        Else
          If IsNull(Txconv2.Text) Then Txconv2.Text = "1"
          Frais2.SetFocus
          Frais2.Select
          Chkfocus = 0
        Endif
        b = 0
        Stop Event
        
      Case 12
        FraisLF2()
        If b = 1 Then
          Frais2.SetFocus
          Frais2.Select
        Else
          Coef2.SetFocus
          Coef2.Select
          Chkfocus = 0
        Endif
        b = 0
        Stop Event
        
      Case 13
        Coeff2()
        If b = 1 Then
          Coef2.SetFocus
          Coef2.Select
        Else
          Calc_Percent2()
          Pvht2.Text = Format$(Val(prvt2.Text) * Val(Coef2.Text), nbdec)
          Tva_calcul2()
          Arrondi2()
          Calc_Percent2()
          Calc_Marge2()
          Percent2.SetFocus
          Percent2.Select
          Chkfocus = 0
        Endif
        b = 0
        Stop Event
        
      Case 14
        Pourcent2()
        If b = 1 Then
          Percent2.SetFocus
          Percent2.Select
        Else
          Calc_coeff2()
          Pvht2.Text = Format$(Val(prvt2.Text) * Val(Coef2.Text), nbdec)
          Tva_calcul2()
          Arrondi2()
          Calc_Marge2()
          Pvht2.SetFocus
          Pvht2.Select
          Chkfocus = 0
        Endif
        b = 0
        Stop Event
        
      Case 15
        PvhtLF2()
        If b = 1 Then
          Pvht2.SetFocus
          Pvht2.Select
        Else
          PvTTc2.SetFocus
          PvTTc2.Select
          Chkfocus = 0
        Endif
        b = 0
        Stop Event
        
      Case 16
        PvTTcLF2()
        If b = 1 Then
          PvTTc2.SetFocus
          PvTTc2.Select
        Else
          pbht2.SetFocus
          pbht2.Select
          Chkfocus = 0
        Endif
        b = 0
        Stop Event
        
    End Select
  Endif
  
  If key.code = key.F1 Then
    Button1_Click()
    Stop Event
  Endif
  
End

