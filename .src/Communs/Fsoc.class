Public Sub Soclist_Click()

  If Soclist.Item[0] <> Settings["/Soc"] Then
    Utils.db.Close
    Utils.db.name = Soclist.Item[2]
    Utils.db.Open
    Utils.db.exec("SET NAMES 'latin1'")
    cs = Soclist.Item[0]
    If Not IsNull(Utils.db.Login) Then Shell "mysql -h " & Settings["/dbase/Host"] & " " & " -u " & Utils.db.Login & " -p" & Utils.db.Password & " -e \"use " & Utils.db.Name & ";" & "SET @@global.sql_mode= NO_ENGINE_SUBSTITUTION " & "; \"" Wait
    Maj_Zones()
    soc = code.Text
    Settings["/Soc"] = soc
    Settings["/intitule"] = intitule.Text
    Settings["/dbase/Name"] = Soclist.Item[2]
    Settings.Save
    Settings.Reload
    Start.Societe = soc
    Syntheses.Message(" Vous venez de sélectionner la société " & Soc & " , " & intitule.Text)
    message.Warning(" Vous venez de sélectionner la société " & Soc & " " & intitule.Text & " ! \n Celle-ci sera donc votre société de travail par défaut...\nLaurux va redémarrer")
    chkNbase = False
    Utils.fermeTunnel()
    Utils.db.Close
    Try Me.close
    Try Fmenu.delete
    Try Fmenuc.delete
    Try Fmenuf.delete
    Start.Main()
  Else
    Maj_Zones()
  Endif

End
