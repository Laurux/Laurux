Public Sub SetObserver(hCont As Container)

  Dim hObs As Observer
  Dim o As Control

  For Each o In hCont.Children
    If o Is Label Then
      hobs = New Observer(o) As "OBS"
      o.Mouse = mouse.Pointing
    Endif
    If o Is Container Then SetObserver(o)
  Next

End

