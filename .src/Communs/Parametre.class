Public Sub datec_LostFocus()
  
  With Utils
    datec.text = .Cdate_calc(datec.text)
    datec.Text = .Cdate_aff(datec.Text)
    If datec.Text = "00.00.0:00" Or If datec.Text = ".." Then
      B = 1
    Else
      'mouvmt()
    Endif
  End With
  
End

