  Dim jal As String
  
  jal = "Fiches_Journaux"
  If Settings["/Soc" & Start.Societe & "/Caisse"] = True Then
    Jnal = Utils.db.Exec("SELECT * FROM " & Jal & " WHERE code_jo = &1", jrnal4.Text)
    If Not Jnal.Available Then
      If son Then
        Music.Play
      Endif
      If Message.Question("Ce journal n'existe pas !\nVoulez-vous le créer ?", "Oui", "Non") = 1 Then
        Fjournaux.show
      Else
        jrnal4.SetFocus
        b = 1
      Endif
    Else
      If Jnal!type_jo <> "VE" Then
        Message.Warning("Ce journal n'est pas un journal de ventes !")
        b = 1
      Else
        b = 0
      Endif
    Endif
  Endif
  
End

