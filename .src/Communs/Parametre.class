    smc = .Cdate_calc(smc)
    smc = .Cdate_aff(smc)
    mc.text = Mid$(smc, 4, 3) & Right$(smc, 4)
    If mc.Text = "00.0:00" Then
      B = 1
      Mc.SetFocus
      Mc.Select
    Endif
  End With
  
End

