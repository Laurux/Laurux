Public Sub Chk()

  If InStr("0123456789,./", Key.Text) = 0 Then
    If key.code <> key.BackSpace And Key.Code <> Key.tab And Key.Code <> Key.Delete And Key.code <> Key.enter And Key.code <> Key.return And Key.Code <> Key.Control Then
      Stop Event
    Endif
  Endif

End

