Public Sub Verif_Compte(Compte As String) As Boolean
  
  Dim Cpt As Result
  
  Cpt = Utils.db.Exec("SELECT * FROM " & Cbase.Table("TabComptes") & " WHERE compte_cc = &1", Compte)
  If Not Cpt.Available Then
    Return False
  Else
    Return True
  Endif
  
End

