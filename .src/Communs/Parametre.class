Public Sub Btn_Click()
  
  Select Case Last.tag
      
    Case 1
      Button3_Click()
      
    Case 2
      Button2_Click()
      
    Case 3
      Button2_Click()
      If b <> 1 Then Me.Close
      
  End Select
  
End

Public Sub Form_Close()
  
  Button2_Click()
  If b <> 1 Then
    Me.Close
  Else
    Stop Event
  Endif
  
End

Public Sub Par_GotFocus()
  
