        posy = 32
        pdf.level6J()
        posy += 6
        Centralisation()
        'Inc ipage
        pdf.BasPage2(ipage)
        iPage = 1
        raz()
        Create_table()
      Else
        iPage = 1
        raz()
      Endif
      'Endif
      raz()
      Solde = 0
      pdf.BasPage2(ipage)
      pdf.Output(Filename, False)
      Visualiseur.Prog_Editeur(Filename)
    Else
      message(" Aucune ecriture pour cette demande", "OK")
    Endif
  End With
  Me.Mouse = Mouse.Default
  
End

